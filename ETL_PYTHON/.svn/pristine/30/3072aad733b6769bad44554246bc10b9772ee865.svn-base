def GetType22FirstSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name):
    sql = """
    INSERT  /*+ append */  INTO APS_TR_TYPE22 (                                                              
            GUID, MODULE_ID, TOOLG_ID, PPID, PROCESS_START, SCANNER_START, SCANNER_END, PROCESS_END, OPERATION_COMPLETE, QTY, PARTKEY
    )                                                                                                                       
     WITH SOURCE_TYPE22 AS (                                                                                                       
         SELECT  '{uuid}' AS GUID, 
                TL.MODULE_ID,                                                                          
                TL.TOOLG_ID,                                                                                                                                                                                    
                PT.PPID,                                                                                                    
                PT.PROCESS_START AS PROCESS_START,                                                   
                (CASE WHEN PT.SCANNER_END IS NULL THEN NULL ELSE  STRFTIME(PT.SCANNER_START, '%Y-%m-%d %H:%M:%S') END) AS SCANNER_START,
                (CASE WHEN PT.SCANNER_START IS NULL THEN NULL ELSE  STRFTIME(PT.SCANNER_END, '%Y-%m-%d %H:%M:%S') END) AS SCANNER_END,
                -- TO_CHAR(PT.PROCESS_END,'YYYY-MM-DD HH24:MI:SS') AS PROCESS_END,
                  PT.PROCESS_END AS PROCESS_END,                                          
                -- TO_CHAR(PT.OPERATION_COMPLETE,'YYYY-MM-DD HH24:MI:SS') AS OPERATION_COMPLETE,   
                 PT.OPERATION_COMPLETE AS OPERATION_COMPLETE,                              
                   CASE WHEN TP.TOOL_ID IS NOT NULL AND TP.TOOL_ID <> '' THEN LAG(PROCESS_START,1,NULL) OVER(PARTITION BY PT.EQP_ID,CLAIM_MEMO ORDER BY PROCESS_START)
                   ELSE LAG(PROCESS_START,1,NULL) OVER(PARTITION BY PT.EQP_ID ORDER BY PROCESS_START) END AS NEXT_PROCESS_START, 
                   CASE WHEN TP.TOOL_ID IS NOT NULL AND TP.TOOL_ID <> '' THEN LAG(PROCESS_END,1,NULL) OVER(PARTITION BY PT.EQP_ID,CLAIM_MEMO ORDER BY PROCESS_END)   
                   ELSE LAG(PROCESS_END,1,NULL) OVER(PARTITION BY PT.EQP_ID ORDER BY PROCESS_END) END AS NEXT_PROCESS_END,  
                PT.QTY                                                                                                      
         FROM {tempdb}APS_TMP_TYPE22_RESULT PT                                                                                     
         INNER JOIN {tempdb}APS_TMP_TYPE22_TOOL TL                                                                                 
         ON TL.EQP_ID = PT.EQP_ID      
         LEFT JOIN {tempdb}APS_TMP_TYPE22_PARMODE TP  
         ON TP.TOOL_ID = PT.EQP_ID                                                                                       
        WHERE PT.PROCESS_END IS NOT NULL AND PT.PROCESS_END <> '' AND PT.PROCESS_START IS NOT NULL AND PT.PROCESS_START <> ''                                             
     ),
     --modify by xiecheng for version up
      TYPE22 AS (                                                                                                             
             SELECT GUID, MODULE_ID,                                                                                            
                    TOOLG_ID,                                                                                                                                                                                                     
                    PPID,                                                                                                       
        -- 如果后一批的process_start 在前一批的process_start到process_end里边 则后一批的processs_start 取前一批的process_end的值
                    CASE WHEN SCANNER_START IS NULL AND SCANNER_END IS NULL AND PROCESS_START > NEXT_PROCESS_START              
                         AND PROCESS_START < NEXT_PROCESS_END AND NEXT_PROCESS_END < PROCESS_END THEN  STRFTIME(NEXT_PROCESS_END, '%Y-%m-%d %H:%M:%S')         
                               ELSE STRFTIME(PROCESS_START, '%Y-%m-%d %H:%M:%S') END AS PROCESS_START,                        
                    SCANNER_START,                                                                                              
                    SCANNER_END,                                                                                                
                    PROCESS_END AS PROCESS_END,                                                
                    OPERATION_COMPLETE,                                                                                         
                    QTY                                                                                                         
             FROM SOURCE_TYPE22 PT                                                                                                
         )                                                                                                                                                
     SELECT  GUID,                                                                                                          
             MODULE_ID,                                                                                                     
             TOOLG_ID,                                                                                                      
             PPID,                                                                                                  
             PROCESS_START,                                                                                                 
             SCANNER_START,                                                                                                 
             SCANNER_END,                                                                                                   
             PROCESS_END,                                                                                                   
             OPERATION_COMPLETE,                                                                                            
             QTY,                                                                                                           
             -- TO_DATE(PROCESS_START, 'YYYY-MM-DD HH24:MI:SS') AS PARTKEY
             PROCESS_START AS PARTKEY
     FROM  TYPE22 T                                                                                                                                                                                                                                                                                          
    """.format(uuid=uuid, tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TR_TYPE22",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TR_TYPE22")


def GetType22OtherSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name):
    sql = """
    INSERT  /*+ append */  INTO APS_TR_TYPE22 (                                                              
            GUID, MODULE_ID, TOOLG_ID, PPID, PROCESS_START, SCANNER_START, SCANNER_END, PROCESS_END, OPERATION_COMPLETE, QTY, PARTKEY
    )                                                                                                                       
     WITH SOURCE_TYPE22 AS (                                                                                                       
         SELECT  '{uuid}' AS GUID, 
                TL.MODULE_ID,                                                                          
                TL.TOOLG_ID,                                                                                                                                                                                    
                PT.PPID,                                                                                                    
                PT.PROCESS_START AS PROCESS_START,                                                   
                (CASE WHEN PT.SCANNER_END IS NULL THEN NULL ELSE  STRFTIME(PT.SCANNER_START, '%Y-%m-%d %H:%M:%S') END) AS SCANNER_START,
