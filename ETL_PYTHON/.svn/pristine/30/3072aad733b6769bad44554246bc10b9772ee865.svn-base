def create_temp_table(duck_db):
    table_type = ""
    if not config.g_debug_mode:
        table_type = 'TEMP'
    '''
    这里在内存中创建使用到的临时表
    '''
    sql = """
    create {table_type} table APS_TMP_TYPE22_TOOL
    (
      toolg_id  VARCHAR(64),
      eqp_id    VARCHAR(64),
      module_id VARCHAR(64)
    )
    """.format(table_type=table_type)
    duck_db.sql(sql)
    sql = """
    create {table_type} table APS_TMP_TYPE22_HIST
    (
      eqp_id             VARCHAR(64),
      ppid               VARCHAR(64),
      qty                VARCHAR(64),
      lot_id             VARCHAR(64),
      ctrl_job           VARCHAR(64),
      process_start      TIMESTAMP,
      scanner_start      TIMESTAMP,
      scanner_end        TIMESTAMP,
      process_end        TIMESTAMP,
      operation_complete TIMESTAMP,
      claim_memo         VARCHAR(4000)
    )
    """.format(table_type=table_type)
    duck_db.sql(sql)
    sql = """
    create {table_type} table APS_TMP_TYPE22_RESULT
    (
      eqp_id             VARCHAR(64),
      ppid               VARCHAR(64),
      qty                VARCHAR(64),
      process_start      TIMESTAMP,
      scanner_start      TIMESTAMP,
      scanner_end        TIMESTAMP,
      process_end        TIMESTAMP,
      operation_complete TIMESTAMP,
      claim_memo         VARCHAR(4000)
    )
    """.format(table_type=table_type)
    duck_db.sql(sql)

    sql = """
        create {table_type} table APS_TMP_TYPE22_PARMODE
        (
          tool_id VARCHAR(64)
        )
        """.format(table_type=table_type)
    duck_db.sql(sql)

    sql = """
          create {table_type} table APS_TMP_TYPE22_HIST_QTY
          (
            ctrl_job   VARCHAR(64),
            qty        decimal,
            claim_memo VARCHAR(4000)
          )
          """.format(table_type=table_type)
    duck_db.sql(sql)


def getEtlToolSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name):
    sql = """
    INSERT INTO {tempdb}APS_TMP_TYPE22_TOOL (                                           
       TOOLG_ID, EQP_ID, MODULE_ID                                                             
    )                                                                          
         SELECT MAX(EQP_G) AS TOOLG_ID, EQP_ID,                                                                             
         MAX(REAL_MODULE) AS MODULE_ID                                                                                      
         FROM APS_SYNC_ETL_TOOL.APS_SYNC_ETL_TOOL                                                                     
         WHERE EQP_CATEGORY <> 'Measurement'                                         
         GROUP BY EQP_ID                     
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_TYPE22_TOOL",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_TYPE22_TOOL")


def getHistSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name):
    sql = """
    INSERT INTO {tempdb}APS_TMP_TYPE22_HIST (                                                
      EQP_ID, PPID, QTY, LOT_ID, CTRL_JOB, PROCESS_START, SCANNER_START, SCANNER_END, PROCESS_END, OPERATION_COMPLETE,CLAIM_MEMO
    )                                                                          
         SELECT F.EQP_ID,                                                                                                   
                F.PH_RECIPE_ID AS PPID,                                                                                     
                F.CUR_WAFER_QTY AS QTY,                                                                                     
                F.LOT_ID,                                                                                                   
                F.CTRL_JOB,                                                                                                 
                CASE WHEN OPE_CATEGORY='ProcessStart' THEN F.CLAIM_TIME ELSE NULL END AS PROCESS_START,                     
                CASE WHEN OPE_CATEGORY='ScannerStart' THEN F.CLAIM_TIME ELSE NULL END AS SCANNER_START,                     
                CASE WHEN OPE_CATEGORY='ScannerEnd' THEN F.CLAIM_TIME ELSE NULL END AS SCANNER_END,                         
                CASE WHEN OPE_CATEGORY='ProcessEnd' THEN F.CLAIM_TIME ELSE NULL END AS PROCESS_END,                         
                CASE WHEN OPE_CATEGORY='OperationComplete' THEN F.CLAIM_TIME ELSE NULL END AS OPERATION_COMPLETE,
                F.CLAIM_MEMO            
         FROM APS_MID_FHOPEHS_VIEW.APS_MID_FHOPEHS_VIEW F                                                                    
         WHERE F.OPE_CATEGORY IN ('ProcessStart','ProcessEnd','ScannerEnd','ScannerStart','OperationComplete')              
         -- AND F.PARTKEY >= DATE_TRUNC('second', CURRENT_TIMESTAMP) - INTERVAL '30 day'     PARTKEY字段不存在 
         AND F.CLAIM_TIME >= DATE_TRUNC('second', CURRENT_TIMESTAMP) - INTERVAL '50 day'
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_TYPE22_HIST",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_TYPE22_HIST")


def getHistQtySql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name):
    sql = """
       INSERT INTO {tempdb}APS_TMP_TYPE22_HIST_QTY (                                           
        CTRL_JOB, QTY, CLAIM_MEMO                                                         
        )                                                                      
             SELECT F.CTRL_JOB,                                                    
                    SUM(cast(F.QTY as decimal)) AS QTY,                                  
                    MAX(F.CLAIM_MEMO) AS CLAIM_MEMO                          
             FROM  {tempdb}APS_TMP_TYPE22_HIST F                                       
             WHERE F.OPERATION_COMPLETE IS NOT NULL AND F.OPERATION_COMPLETE <> ''                    
             GROUP BY F.CTRL_JOB                                                                                        
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_TYPE22_HIST_QTY",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_TYPE22_HIST_QTY")


def getHistResultSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name):
    sql = """
        INSERT INTO {tempdb}APS_TMP_TYPE22_RESULT (                                           
           EQP_ID, PPID, QTY, PROCESS_START, SCANNER_START, SCANNER_END, PROCESS_END, OPERATION_COMPLETE, CLAIM_MEMO                                                    
        )                                                                          
             SELECT MAX(F.EQP_ID) AS EQP_ID,                                                                                    
                    MAX(F.PPID) AS PPID,                                                                                        
                    MAX(Q.QTY) AS QTY,                                                                                          
                    MAX(F.PROCESS_START) AS PROCESS_START,                                                                      
                    MAX(F.SCANNER_START) AS SCANNER_START,                                                                      
                    MAX(F.SCANNER_END) AS SCANNER_END,                                                                          
                    MAX(F.PROCESS_END) AS PROCESS_END,                                                                          
                    MAX(F.OPERATION_COMPLETE) AS OPERATION_COMPLETE,
                    MAX(Q.CLAIM_MEMO) AS CLAIM_MEMO                                                             
             FROM  {tempdb}APS_TMP_TYPE22_HIST F     
             INNER JOIN {tempdb}APS_TMP_TYPE22_HIST_QTY Q
                ON Q.CTRL_JOB = F.CTRL_JOB                                                                                    
             GROUP BY F.CTRL_JOB                                                                                     
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_TYPE22_RESULT",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_TYPE22_RESULT")


