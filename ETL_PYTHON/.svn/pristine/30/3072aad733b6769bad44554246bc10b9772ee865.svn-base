                   CASE WHEN TP.TOOL_ID IS NOT NULL AND TP.TOOL_ID <> '' THEN LAG(PROCESS_END,1,NULL) OVER(PARTITION BY PT.EQP_ID,CLAIM_MEMO ORDER BY PROCESS_END)   
                   ELSE LAG(PROCESS_END,1,NULL) OVER(PARTITION BY PT.EQP_ID ORDER BY PROCESS_END) END AS NEXT_PROCESS_END,  
                PT.QTY                                                                                                      
         FROM {tempdb}APS_TMP_TYPE22_RESULT PT                                                                                     
         INNER JOIN {tempdb}APS_TMP_TYPE22_TOOL TL                                                                                 
         ON TL.EQP_ID = PT.EQP_ID      
         LEFT JOIN {tempdb}APS_TMP_TYPE22_PARMODE TP  
         ON TP.TOOL_ID = PT.EQP_ID                                                                                       
        WHERE PT.PROCESS_END IS NOT NULL AND PT.PROCESS_END <> '' AND PT.PROCESS_START IS NOT NULL AND PT.PROCESS_START <> ''                                            
     ),
     --modify by xiecheng for version up
      TYPE22 AS (                                                                                                             
             SELECT GUID, MODULE_ID,                                                                                            
                    TOOLG_ID,                                                                                                                                                                                                     
                    PPID,                                                                                                       
        -- 如果后一批的process_start 在前一批的process_start到process_end里边 则后一批的processs_start 取前一批的process_end的值
                    CASE WHEN SCANNER_START IS NULL AND SCANNER_END IS NULL AND PROCESS_START > NEXT_PROCESS_START              
                         AND PROCESS_START < NEXT_PROCESS_END AND NEXT_PROCESS_END < PROCESS_END THEN  STRFTIME(NEXT_PROCESS_END, '%Y-%m-%d %H:%M:%S')         
                               ELSE STRFTIME(PROCESS_START, '%Y-%m-%d %H:%M:%S') END AS PROCESS_START,                        
                    SCANNER_START,                                                                                              
                    SCANNER_END,                                                                                                
                    PROCESS_END AS PROCESS_END,                                                
                    OPERATION_COMPLETE,                                                                                         
                    QTY                                                                                                         
             FROM SOURCE_TYPE22 PT                                                                                                
         )                                                                                                                                                
     SELECT  GUID,                                                                                                          
             MODULE_ID,                                                                                                     
             TOOLG_ID,                                                                                                      
             PPID,                                                                                                  
             PROCESS_START,                                                                                                 
             SCANNER_START,                                                                                                 
             SCANNER_END,                                                                                                   
             PROCESS_END,                                                                                                   
             OPERATION_COMPLETE,                                                                                            
             QTY,                                                                                                           
             -- TO_DATE(PROCESS_START, 'YYYY-MM-DD HH24:MI:SS') AS PARTKEY
             PROCESS_START AS PARTKEY
     FROM  TYPE22 T     
     WHERE                                                                                                    
         NOT EXISTS (                                                                                               
             SELECT MODULE_ID  FROM LAST_APS_TR_TYPE22.APS_TR_TYPE22 T22 WHERE T22.MODULE_ID = T.MODULE_ID                             
             AND T22.TOOLG_ID = T.TOOLG_ID                                                                          
             AND T22.PPID = T.PPID AND T22.PROCESS_START = T.PROCESS_START                                          
             AND T22.QTY = T.QTY AND T22.PROCESS_END = T.PROCESS_END                                                
         )                                                                                                                                                                                                                                                                            
    """.format(uuid=uuid, tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TR_TYPE22",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TR_TYPE22")


def execute():
    ###############################################################
    ### 以下参数必须定义
    ### ETL_Proc_Name    : ETL 名称
    ### current_time     ：请直接拷贝
    ### current_time_short ：请直接拷贝
    ### uuid             ：请直接拷贝
    ### target_table     : 该ETL输出表名
    ### used_table_list  : 该ETL使用到的，参考到的表名(中间表不算)
    ### target_table_sql ： 该ETL输出表定义SQL
    ###############################################################
    ETL_Proc_Name = "APS_ETL_BR.APS_TR_TYPE22_30M"
    current_time = my_date.date_time_second_str()
    current_time_short = my_date.date_time_second_short_str()
    uuid = my_oracle.UUID()

    target_table = "APS_TR_TYPE22"
    used_table_list = ['APS_SYNC_ETL_TOOL', 'APS_MID_FHOPEHS_VIEW', 'APS_ETL_TOOL']
    target_table_sql = """
        create table {}APS_TR_TYPE22
        (
          module_id          VARCHAR(60) not null,
          toolg_id           VARCHAR(60) not null,
          ppid               VARCHAR(60) not null,
          process_start      VARCHAR(60) not null,
          scanner_start      VARCHAR(60),
          scanner_end        VARCHAR(60),
          process_end        VARCHAR(60),
          qty                VARCHAR(60),
          guid               VARCHAR(60) not null,
          partkey            VARCHAR(60) not null,
          operation_complete VARCHAR(64)
        )
    """.format("")
    target_db_file = my_duck.get_target_file_name(target_table, current_time_short)
    # -------------------------- 内存模式改成文件模式
    _temp_db_path = os.path.join(config.g_mem_speed_etl_output_path, target_table, "inprocess")
    if not os.path.exists(_temp_db_path):
        os.makedirs(_temp_db_path)

    temp_db_file = os.path.join(_temp_db_path, target_table + "_" + current_time_short + "_temp.db")
    # 处理中文件
    in_process_db_file = os.path.join(_temp_db_path, target_table + "_" + current_time_short + ".db")
    # 结果文件
    target_db_file = os.path.join(config.g_mem_etl_output_path, target_table,
                                  target_table + "_" + current_time_short + ".db")
    # --------------------------

    oracle_conn = None
    try:
        oracle_conn = my_oracle.oracle_get_connection()
        # 开始日志
        my_oracle.StartCleanUpAndLog(oracle_conn, ETL_Proc_Name, current_time)
        # -------------------------- 内存模式改成文件模式
        # 创建DuckDB
        duck_db_memory = my_duck.create_duckdb_in_file(_temp_db_path, in_process_db_file, target_table_sql)
        duck_db_memory.sql('SET threads TO 4')
        if not os.path.exists(os.path.join(config.g_mem_speed_etl_output_path, 'duck_temp', uuid)):
            os.makedirs(os.path.join(config.g_mem_speed_etl_output_path, 'duck_temp', uuid))
        duck_db_memory.execute(
            "SET temp_directory='{}'".format(os.path.join(config.g_mem_speed_etl_output_path, 'duck_temp', uuid)))

        if not config.g_debug_mode:
            create_temp_table(duck_db_memory)
        else:
            duck_db_temp = my_duck.create_duckdb_for_temp_table(_temp_db_path, temp_db_file)
            # 创建Temp表
            create_temp_table(duck_db_temp)
            duck_db_temp.commit()
            duck_db_temp.close()
            my_duck.attach_temp_db_write_able(duck_db_memory, "TEMPDB", temp_db_file)
        # --------------------------
        # Attach用到的表
        my_duck.attach_used_table(oracle_conn, duck_db_memory, used_table_list)
        ################################################################################################################
        ## 以下为业务逻辑
        ################################################################################################################
#       # 获取Hist的数据
        getEtlToolSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name)
        # 获取Chamber Rule规则
        getHistSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name)
        # 按整个CTRL_JOB来看有多少QTY
        getHistQtySql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name)
        # 获取最终的成型数据
        getHistResultSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name)
        # 获取parallel_mode
        getParallelModeEqpSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name)

        # 取上一次的APS_TR_TYPE22版本
        file_name = my_file.get_last_db_file(conn=oracle_conn, table_name="APS_TR_TYPE22")
        # if 取不到
        if file_name is None:
            GetType22FirstSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name)

        # else 取到的情况下
        # 手动 Attach APS_TR_TYPE22 AS LAST_APS_TR_TYPE22
        else:
            my_duck.attach_table(duck_db_memory=duck_db_memory, table_name="LAST_APS_TR_TYPE22",
                                 target_db_file=file_name)

            GetType22OtherSql(duck_db_memory, uuid, current_time, oracle_conn, ETL_Proc_Name)

            sql = """insert into APS_TR_TYPE22 select * from LAST_APS_TR_TYPE22.APS_TR_TYPE22 
                           WHERE CAST(PARTKEY AS TIMESTAMP) >= DATE_TRUNC('second', CURRENT_TIMESTAMP)- INTERVAL '50 day'"""
            duck_db_memory.execute(sql)

            my_duck.detach_table(duck_db=duck_db_memory, db_name="LAST_APS_TR_TYPE22")

        ################################################################################################################
        ## 以上为业务逻辑
        ################################################################################################################
        # 导出到目标文件中
        target_db_file = my_duck.export_result_duck_file_and_close_duck_db_memory2(duck_db_memory,
                                                                                   in_process_db_file=in_process_db_file,
                                                                                   target_table=target_table,
                                                                                   current_time=current_time_short)
        # 写版本号
        my_oracle.HandlingVerControl(oracle_conn, uuid, target_table, target_db_file, current_time_short)
        # 写完成日志
        my_oracle.EndCleanUpAndLog(oracle_conn, ETL_Proc_Name, current_time)
    except Exception as e:
        # 写警告日志
        my_oracle.SaveAlarmLogData(oracle_conn, ETL_Proc_Name, e, target_db_file, 'XETL036')
        logging.exception("{ETL_Proc_Name} 處理出錯 : {e}".format(ETL_Proc_Name=ETL_Proc_Name, e=e))
        # 导出到目标文件中
        my_duck.export_result_duck_file_and_close_duck_db_memory2(duck_db_memory,
                                                                  in_process_db_file=in_process_db_file,
                                                                  target_table=target_table,
                                                                  current_time=current_time_short)
        raise e
    finally:
        oracle_conn.commit()
        oracle_conn.close()
        # 删除TMP目录:LQN:2023/08/21
        if os.path.exists(os.path.join(config.g_mem_etl_output_path, 'duck_temp', uuid)):
            os.remove(os.path.join(config.g_mem_etl_output_path, 'duck_temp', uuid))
        if os.path.exists(temp_db_file) and not config.g_debug_mode:
            os.remove(temp_db_file)
        gc.collect()  # 内存释放


if __name__ == '__main__':
    # 单JOB测试用
    print("start")
    execute()
