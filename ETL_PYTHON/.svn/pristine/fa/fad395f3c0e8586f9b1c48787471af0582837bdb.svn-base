               ,I.PLAN_ID                                                                                               
               ,I.OPE_NO                                                                                                
               ,I.EQP_ID                                                                                                
               ,I.PASS_FLG_PRC, '', '', '', ''                                                                          
         FROM {tempdb}APS_TMP_ETL_RLS_CSFR_IHBT I                                                                             
         INNER JOIN {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_CHIPBODY C ON I.CHIPBODY_PROD = C.CHIPBODY                                  
