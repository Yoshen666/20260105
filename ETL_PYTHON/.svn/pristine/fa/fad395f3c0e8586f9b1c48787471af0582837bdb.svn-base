        """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_CSFRIHBT_5CASE_INHIBIT",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_CSFRIHBT_5CASE_INHIBIT")
    sql = """
         INSERT /*+ append */ INTO {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_5CASE
         (                                                                                                             
          PROD_ID, PLAN_ID, OPE_NO, EQP_ID, PASS_FLG_PRC,                                                              
          PASS_FLG_MFG, PRC_UPT_USER                                                                                   
         )                                                                                                             
         SELECT CT.PROD_ID                                                                                             
               ,CT.PLAN_ID                                                                                             
               ,CT.OPE_NO                                                                                              
               ,CT.EQP_ID                                                                                              
               ,CT.PASS_FLG_PRC                                                                                        
               ,CT.PASS_FLG_MFG                                                                                        
               ,CT.PRC_UPT_USER                                                                                        
             FROM {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_5CASE_INHIBIT CT                                                            
             INNER JOIN {tempdb}APS_TMP_ETL_RLS_PATH_SETTING S                                              
             ON S.TOOL_ID = CT.EQP_ID                                                            
             AND S.USE_FLAG_TYPE = 'Userflag(5)'                                                                                       
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
