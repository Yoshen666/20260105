     LEFT JOIN {tempdb}APS_TMP_ETL_RLS_RTD_PREFER_TOOL PT ON  PT.PRODUCT_ID = W.PROD_ID           
                                  AND PT.OPE_NO = W.TARGET_OPE_NO 
     WHERE W.NO_CH_SET = 'ChSet'                                                          
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_APC_PILOT",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_APC_PILOT")

def mixed(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                  used_table_list=None):
    start_time = time.time()
    # 针对 target wip的 可使用 eqp 和 recipe 和 reticle
    # -- 产出
    # APS_TMP_ETL_RLS_EQPRCPRTC_RESULT
    InsertEQPRecipeReticle2Temp(duck_db_memory=duck_db_memory,
                                                             uuid=uuid,
                                                             current_time=current_time,
                                                             oracle_conn=oracle_conn,
                                                             ETL_Proc_Name=ETL_Proc_Name,
                                                             used_table_dict=used_table_list)

    # RTD DF和WET需要判断同一个Foup只能对应一个Recipe数据，多个则不能排程
    # -- 产出
    # APS_TMP_ETL_RLS_FOUP_RECIPE
    InsertSameFoupRecipeDataView2Temp(duck_db_memory=duck_db_memory,
                                                                   uuid=uuid,
                                                                   current_time=current_time,
                                                                   oracle_conn=oracle_conn,
                                                                   ETL_Proc_Name=ETL_Proc_Name,
                                                                   used_table_dict=used_table_list)

    # 针对ReworkComing的限制，需要找对应母批Run过的机台，找不到就找自己RUN过的机台
    # -- 产出
    # APS_TMP_ETL_RLS_SPLIT_INFO
    # APS_TMP_ETL_RLS_REWORK_COMING
    # APS_TMP_ETL_RLS_REWORK_COMING
    InsertReworkComing2Temp(duck_db_memory=duck_db_memory,
                                                         uuid=uuid,
                                                         current_time=current_time,
                                                         oracle_conn=oracle_conn,
                                                         ETL_Proc_Name=ETL_Proc_Name,
                                                         used_table_dict=used_table_list)

    # 针对APC Split Lot 只允许该站点该机台上RUN，有机台的话自己是Active, 其他机台为NotActive
    # -- 产出
    # APS_TMP_ETL_RLS_APC_SPLIT_INFO
    InsertLotApcSplitInfo2Temp(duck_db_memory=duck_db_memory,
                                                            uuid=uuid,
                                                            current_time=current_time,
                                                            oracle_conn=oracle_conn,
                                                            ETL_Proc_Name=ETL_Proc_Name,
                                                            used_table_dict=used_table_list)

    # 针对 特补Lot的指定机台，只允许该批次站点在该特补机台上RUN
    # -- 产出
    # APS_TMP_ETL_RLS_LOTSPECIAL_RESULT
    InsertLotSpecial2Temp(duck_db_memory=duck_db_memory,
                                                       uuid=uuid,
                                                       current_time=current_time,
                                                       oracle_conn=oracle_conn,
                                                       ETL_Proc_Name=ETL_Proc_Name,
                                                       used_table_dict=used_table_list)

    # csfr inhibit 6种机限summary
    # -- 产出
    # APS_TMP_ETL_RLS_CSFR_IHBT
    InsertCsfrInhibit2Temp(duck_db_memory=duck_db_memory,
                                                        uuid=uuid,
                                                        current_time=current_time,
                                                        oracle_conn=oracle_conn,
                                                        ETL_Proc_Name=ETL_Proc_Name,
                                                        used_table_dict=used_table_list)

    # 已开PATH的机台不属于机限
    # -- 产出
    # APS_TMP_ETL_RLS_CSFR_PATH
    InsertCsfrPath2Temp(duck_db_memory=duck_db_memory,
                                                     uuid=uuid,
                                                     current_time=current_time,
                                                     oracle_conn=oracle_conn,
                                                     ETL_Proc_Name=ETL_Proc_Name,
                                                     used_table_dict=used_table_list)

    # Rework指定机群找出有RPC_FLAG='Y',有Recipe
    # -- 产出
    # APS_TMP_ETL_RLS_LOT_TO_EQPG_INHIBIT
    # APS_TMP_ETL_RLS_LOT_TO_EQPG_RECIPE
    getReworkToEqpGActive(duck_db_memory=duck_db_memory,
                                                       uuid=uuid,
                                                       current_time=current_time,
                                                       oracle_conn=oracle_conn,
                                                       ETL_Proc_Name=ETL_Proc_Name,
                                                       used_table_dict=used_table_list)

    # 虚拟Chamber规则
    getTankRule(duck_db_memory=duck_db_memory,
                                             uuid=uuid,
                                             current_time=current_time,
                                             oracle_conn=oracle_conn,
                                             ETL_Proc_Name=ETL_Proc_Name,
                                             used_table_dict=used_table_list)

    # QTIME等级的处理，剩余Qtime < 2h + 版本间隔时间（默认1h），且qtimeloop的等级为A | B
    getQtimeGradeData(duck_db_memory=duck_db_memory,
                                                   uuid=uuid,
                                                   current_time=current_time,
                                                   oracle_conn=oracle_conn,
                                                   ETL_Proc_Name=ETL_Proc_Name,
                                                   used_table_dict=used_table_list)

    # 除了线性回归，其他RLS数据插入到 rls temp表
    # 先把特补值和APC_Pilot 单独拆开来
    # 去掉NoCHSet的70多W笔数据
    # -- 产出
    # APS_TMP_ETL_RLS_LOT_SPECIAL
    getLotSpecialInhibit(duck_db_memory=duck_db_memory,
                                                      uuid=uuid,
                                                      current_time=current_time,
                                                      oracle_conn=oracle_conn,
                                                      ETL_Proc_Name=ETL_Proc_Name,
                                                      used_table_dict=used_table_list)

    # 先把特补值和APC_Pilot 单独拆开来
    # -- 产出
    # APS_TMP_ETL_RLS_APC_PILOT
    getApcPliotInhibit(duck_db_memory=duck_db_memory,
                                                    uuid=uuid,
                                                    current_time=current_time,
                                                    oracle_conn=oracle_conn,
                                                    ETL_Proc_Name=ETL_Proc_Name,
                                                    used_table_dict=used_table_list)

    # -- 产出
    # APS_TMP_ETL_RLS_RTD_QTIME
    getRtdQtimeInhibit(duck_db_memory=duck_db_memory,
                                                    uuid=uuid,
                                                    current_time=current_time,
                                                    oracle_conn=oracle_conn,
                                                    ETL_Proc_Name=ETL_Proc_Name,
                                                    used_table_dict=used_table_list)

    # RTD提供的Coming wip预估不准，Qtime中间站点宇清无法知道Path的数据情况，给的RTDQtimePathIssue逻辑
    getPHRtdQtimeInhibit(duck_db_memory=duck_db_memory,
                                                      uuid=uuid,
                                                      current_time=current_time,
                                                      oracle_conn=oracle_conn,
                                                      ETL_Proc_Name=ETL_Proc_Name,
                                                      used_table_dict=used_table_list)

    # CHAMBER RULE的最终串接
    getChamberRuleInhibit(duck_db_memory=duck_db_memory,
                                                       uuid=uuid,
                                                       current_time=current_time,
                                                       oracle_conn=oracle_conn,
                                                       ETL_Proc_Name=ETL_Proc_Name,
                                                       used_table_dict=used_table_list)

    # 重大异常LOT卡控
    getAnomalyEwsLot(duck_db_memory=duck_db_memory,
                                                  uuid=uuid,
                                                  current_time=current_time,
                                                  oracle_conn=oracle_conn,
                                                  ETL_Proc_Name=ETL_Proc_Name,
                                                  used_table_dict=used_table_list)

    # 针对MONITOR userFlag的卡控
    getMonitorRecipeInhibit(duck_db_memory=duck_db_memory,
                                                         uuid=uuid,
                                                         current_time=current_time,
                                                         oracle_conn=oracle_conn,
                                                         ETL_Proc_Name=ETL_Proc_Name,
                                                         used_table_dict=used_table_list)



    end_time = time.time()
    print("Mix执行时间", str(end_time - start_time) + "秒")
    return 0


