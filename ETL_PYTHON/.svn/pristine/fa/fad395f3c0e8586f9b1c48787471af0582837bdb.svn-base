def getAnnotationResultData(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                            used_table_dict=None):
    sql = """
    INSERT  INTO {tempdb}APS_TMP_ETL_RLS_ANNOTATION_RESULT (  
        LOT_ID, PROD_ID, PLAN_ID, STEP_ID, ENTRY_STEP_ID, RETN_STEP_ID, MAIN_PLAN_ID,OPE_NO, TOOLG_ID, TOOLG_TYPE
    )  
    WITH BRANCH_DATA AS (                                                             
        SELECT W.LOT_ID,W.TARGET_PLAN_ID,W.MIN_STEP_ID,W.MAX_STEP_ID,W.PROD_ID          
        FROM {tempdb}APS_TMP_ETL_RLS_ANNOTATION_MAX_STEP W                                          
        INNER JOIN {tempdb}APS_TMP_ETL_RLS_ANNOTATION_FLOW S                                       
        ON S.MAINPD_ID = W.TARGET_PLAN_ID                                               
        AND S.PRODSPEC_ID = W.PROD_ID                                                   
        AND S.LOT_ID = W.LOT_ID                                                         
        AND S.STEP_ID >= MIN_STEP_ID                                                    
        AND S.STEP_ID <= MAX_STEP_ID                                                    
    )                                                                                 
    SELECT DISTINCT D.LOT_ID,F.PROD_ID,F.PLAN_ID,F.STEP_ID,                                    
           F.ENTRY_STEP_ID,F.RETN_STEP_ID,F.MAIN_PLAN_ID,                                    
           F.LAYER || '.' || F.STAGE AS OPE_NO,F.TOOLG_ID,                                   
           F.TOOLG_TYPE                                                                      
    FROM APS_ETL_FLOW.APS_ETL_FLOW F                                      
    INNER JOIN BRANCH_DATA D                                                          
    ON F.MAIN_PLAN_ID = D.TARGET_PLAN_ID                                              
    AND F.ENTRY_STEP_ID >= D.MIN_STEP_ID                                              
    AND F.ENTRY_STEP_ID <= D.MAX_STEP_ID                                              
    AND F.PROD_ID = D.PROD_ID                                                                                      
    AND F.MAIN_PLAN_ID IS NOT NULL AND F.MAIN_PLAN_ID <> ''  
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_ANNOTATION_RESULT",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_ANNOTATION_RESULT")


def getAnnotationAllData(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                         used_table_dict=None):
    sql = """
    INSERT  INTO {tempdb}APS_TMP_ETL_RLS_ANNOTATION_ALL (  
        PARENT_ID, LOT_ID, PRODSPEC_ID, PLAN_ID, TARGET_PLAN_ID, OPE_NO, OPE_SEQ, TARGET_OPE_NO, 
