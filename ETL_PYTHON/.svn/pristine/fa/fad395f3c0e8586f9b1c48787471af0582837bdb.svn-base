    # 相同的 Product, OpeNo, Equipment的 MainFlow的 Pass_flag_Prc = 'Y'的 视为 Active
    sql = """
      INSERT /*+ append */ INTO {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_3CASE
     (                                               		  						            
       PROD_ID, PLAN_ID, OPE_NO, EQP_ID, PASS_FLG_PRC, PASS_FLG_MFG                                                         
