def InsertOnhandWip2Temp(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                         used_table_dict=None):
    # 由于etl_wip 有重复数据，所以先过滤重复数据
    sql = """
        INSERT INTO {tempdb}APS_TMP_ETL_RLS_WIP_SOURCE
        (  LOT_ID,                             
           PRODSPEC_ID,                    
           MAINPD_ID,                      
           OPE_NO,                         
           LOT_TYPE,                       
           LOT_FINISHED_STATE,             
           CASSETE_ID,                     
           WAFER_QTY,                      
           PRIORITY_CLASS,                 
           LOT_PROC_STATE,                 
           LAST_OPE_COMP_TIME,             
           EQP_ID,                         
           OPE_NO_START_TIME,              
           LAST_PROCESS_START_TIME,        
           TECH_ID,                        
           CUSTOMER_ID,                    
           PARENT_LOT_ID,                  
           TRANS_STATE,                    
           FLOWBATCH_ID,                       
           SUB_LOT_TYPE,
           lot_inv_state)
        SELECT LOT_ID,                             
               MAX(PRODSPEC_ID) AS PRODSPEC_ID,                    
               MAX(MAINPD_ID) AS MAINPD_ID,                      
               MAX(OPE_NO) AS OPE_NO,                         
               MAX(LOT_TYPE) AS LOT_TYPE,                       
               MAX(LOT_FINISHED_STATE) AS LOT_FINISHED_STATE,             
               MAX(CASSETE_ID) AS CASSETE_ID,                     
               MAX(WAFER_QTY) AS WAFER_QTY,                      
               MAX(PRIORITY_CLASS) AS PRIORITY_CLASS,                 
               MAX(LOT_PROC_STATE) AS LOT_PROC_STATE,                 
               MAX(LAST_OPE_COMP_TIME) AS LAST_OPE_COMP_TIME,             
               MAX(EQP_ID) AS EQP_ID,                         
               MAX(OPE_NO_START_TIME) AS OPE_NO_START_TIME,              
               MAX(LAST_PROCESS_START_TIME) AS LAST_PROCESS_START_TIME,        
               MAX(TECH_ID) AS TECH_ID,                        
               MAX(CUSTOMER_ID) AS CUSTOMER_ID,                    
               MAX(PARENT_LOT_ID) AS PARENT_LOT_ID,                  
               MAX(TRANS_STATE) AS TRANS_STATE,                    
               MAX(FLOWBATCH_ID) AS FLOWBATCH_ID,                       
               MAX(SUB_LOT_TYPE) AS SUB_LOT_TYPE,
               MAX(lot_inv_state) as lot_inv_state
        FROM APS_SYNC_WIP.APS_SYNC_WIP
        GROUP BY LOT_ID
        """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_WIP_SOURCE",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_WIP_SOURCE")

    '''Onhand Wip 先插入到 wip temp 表'''
    # Onhand Wip 先插入到 wip temp 表
    sql = """
    INSERT  INTO {tempdb}APS_TMP_ETL_RLS_WIP_V 
    (	LOT_ID,                             
           PRODSPEC_ID,                    
           MAINPD_ID,                      
           OPE_NO,                         
           LOT_TYPE,                       
           LOT_FINISHED_STATE,             
           CASSETE_ID,                     
           WAFER_QTY,                      
           PRIORITY_CLASS,                 
           LOT_PROC_STATE,                 
           LAST_OPE_COMP_TIME,             
           EQP_ID,                         
           OPE_NO_START_TIME,              
           LAST_PROCESS_START_TIME,        
           TECH_ID,                        
           CUSTOMER_ID,                    
           PARENT_LOT_ID,                  
           TRANS_STATE,                    
           BATCH_ID,                       
           SUB_LOT_TYPE                    
    )                                      
    WITH PH_WIP AS(                                                                       
         SELECT * FROM {tempdb}APS_TMP_ETL_RLS_WIP_SOURCE                                
         WHERE   OPE_NO LIKE '%.PP%' OR OPE_NO LIKE '%NPR10'     
    ),                                                                                    
    NOT_PH_WIP AS (                                                                       
         SELECT * FROM {tempdb}APS_TMP_ETL_RLS_WIP_SOURCE                                
         WHERE  OPE_NO NOT LIKE '%.PP%' AND OPE_NO NOT LIKE '%NPR10'                       
    )                                                                                     
    SELECT W.LOT_ID,                                                                      
           W.PRODSPEC_ID,                                                                 
           W.MAINPD_ID,                                                                   
           W.OPE_NO,                                                                      
           W.LOT_TYPE,                                                                    
           W.LOT_FINISHED_STATE,                                                          
           W.CASSETE_ID,                                                                  
           W.WAFER_QTY,                                                                   
           W.PRIORITY_CLASS,                                                              
           W.LOT_PROC_STATE,                                                              
           W.LAST_OPE_COMP_TIME,                                                          
           W.EQP_ID,                                                                      
           W.OPE_NO_START_TIME,                                                           
           W.LAST_PROCESS_START_TIME,                                                     
           W.TECH_ID,                                                                     
           W.CUSTOMER_ID,                                                                 
           W.PARENT_LOT_ID,                                                               
           W.TRANS_STATE,                                                                 
           W.FLOWBATCH_ID,                                                                
           W.SUB_LOT_TYPE                                                                 
    FROM PH_WIP W                                                                         
    WHERE 1=1                                                                             
    and lot_inv_state <> 'NonProBank'                                                     
    and (SUBSTRING(lot_id,1,1)='N' or (SUBSTRING(lot_id,1,1)='R' AND ope_no='1R.NPR10'))        
    UNION                                                                                 
    SELECT W.LOT_ID,                                                                      
           W.PRODSPEC_ID,                                                                 
           W.MAINPD_ID,                                                                   
           W.OPE_NO,                                                                      
           W.LOT_TYPE,                                                                    
           W.LOT_FINISHED_STATE,                                                          
           W.CASSETE_ID,                                                                  
           W.WAFER_QTY,                                                                   
           W.PRIORITY_CLASS,                                                              
           W.LOT_PROC_STATE,                                                              
           W.LAST_OPE_COMP_TIME,                                                          
           W.EQP_ID,                                                                      
           W.OPE_NO_START_TIME,                                                           
           W.LAST_PROCESS_START_TIME,                                                     
           W.TECH_ID,                                                                     
           W.CUSTOMER_ID,                                                                 
           W.PARENT_LOT_ID,                                                               
           W.TRANS_STATE,                                                                 
           W.FLOWBATCH_ID,                                                                
           W.SUB_LOT_TYPE                                                                 
    FROM NOT_PH_WIP W                                                                     
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_WIP_V",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_WIP_V")
    sql = """
    INSERT INTO {tempdb}APS_TMP_ETL_RLS_DYB_RETURN_LOT 
    (                                                                                                                                                   
       LOT_ID, PRODSPEC_ID, CURRENT_OPE_NO, OPENO, DYB_MAINPD_ID, RETURN_MAINPD_ID, RETURN_OPE_NO                                                       
      ,LOT_TYPE, CASSETE_ID, WAFER_QTY, PRIORITY_CLASS, LOT_PROC_STATE, LAST_OPE_COMP_TIME                                                              
      ,EQP_ID, OPE_NO_START_TIME, LAST_PROCESS_START_TIME, TECH_ID, CUSTOMER_ID, PARENT_LOT_ID, TRANS_STATE, BATCH_ID,SUB_LOT_TYPE                      
    )                                                                                                                                                   
    SELECT W.LOT_ID, W.PRODSPEC_ID, W.OPE_NO, A.OPENO, A.DYB_MAINPD_ID, A.MAINPD_ID, A.RETURN_OPE_NO                                                    
          ,W.LOT_TYPE, W.CASSETE_ID, W.WAFER_QTY, W.PRIORITY_CLASS, W.LOT_PROC_STATE, W.LAST_OPE_COMP_TIME                                              
          ,W.EQP_ID, W.OPE_NO_START_TIME, W.LAST_PROCESS_START_TIME, W.TECH_ID, W.CUSTOMER_ID, W.PARENT_LOT_ID, W.TRANS_STATE,W.BATCH_ID,SUB_LOT_TYPE   
    FROM {tempdb}APS_TMP_ETL_RLS_WIP_V W                                                                                                                     
    LEFT JOIN APS_SYNC_FLOW_DYB_RETURN.APS_SYNC_FLOW_DYB_RETURN AS A ON REPLACE(A.LOT_ID, '.', '') = W.LOT_ID AND A.DYB_MAINPD_ID = W.MAINPD_ID  
    WHERE W.MAINPD_ID LIKE 'DYB%'                                                                                                                       
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_DYB_RETURN_LOT",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_DYB_RETURN_LOT")
    # 对DYB RECIPE进行整理
    sql = """
         INSERT INTO {tempdb}APS_TMP_ETL_RLS_DYB_RCP_LOAD(                                                                 
        EQP_ID, LOT_ID, M_RECIPE, PF_PD_ID, PRODSPEC_ID, LCRECIPE_ID, OPE_NO, PHYSICAL_RECIPE_ID, RECIPE_ID_C                                                                               
       )                                                                                            
        WITH DYB_RESULT_DATA AS (                                                                   
          SELECT B.EQP_ID,B.LOT_ID,B.M_RECIPE,                                                      
                 B.PF_PD_ID,B.PRODSPEC_ID,                                                          
                 B.OPE_NO,B.LCRECIPE_ID,                                                            
