    )  
     WITH RLS_DATA AS (                                                                                        
         SELECT W.*,                                                                                           
                ROW_NUMBER() OVER(PARTITION BY W.LOT_ID,W.OPE_SEQ ORDER BY W.TARGET_OPE_SEQ DESC) AS RTN       
         FROM {tempdb}APS_TMP_ETL_RLS_ANNOTATION_RESULT R                                                                 
         INNER JOIN {tempdb}APS_TMP_ETL_RLS_WIP W                                                                 
         ON R.LOT_ID = W.LOT_ID                                                                                
     )                                                                                                         
     SELECT W.PARENT_ID,                                                                                       
            W.LOT_ID,                                                                                          
            W.PRODSPEC_ID,                                                                                     
            W.PLAN_ID,                                                                                         
            R.PLAN_ID AS TARGET_PLAN_ID,                                                                       
            W.OPE_NO,                                                                                          
            W.OPE_SEQ,                                                                                         
            R.OPE_NO AS TARGET_OPE_NO,                                                                         
            R.STEP_ID AS TARGET_OPE_SEQ,                                                                       
            R.TOOLG_ID AS TARGET_TOOLG_ID,                                                                     
            W.LOT_TYPE,                                                                                        
            W.LOT_FINISHED_STATE,                                                                              
            W.FOUP_ID,                                                                                         
            W.WAFER_QTY,                                                                                       
            W.LOT_STATUS,                                                                                      
            W.FLOW_RECIPE,                                                                                     
            W.CREATE_TIME,                                                                                     
            W.TRANS_STATE,                                                                                     
            W.BATCH_ID,                                                                                        
            W.SUB_LOT_TYPE,                                                                                    
            R.TOOLG_TYPE,                                                                                      
            R.ENTRY_STEP_ID,                                                                                   
            R.RETN_STEP_ID                                                                                     
     FROM {tempdb}APS_TMP_ETL_RLS_ANNOTATION_RESULT R                                                                     
     INNER JOIN RLS_DATA W                                                                                     
     ON R.LOT_ID = W.LOT_ID                                                                                    
     AND W.RTN = 1                                                                                             

    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
