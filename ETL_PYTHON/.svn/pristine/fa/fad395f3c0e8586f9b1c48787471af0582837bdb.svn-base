    # case 1 : PRC_FLAG = 'N' and PRC_UPT_USER = 'SPC'
    sql = """
    INSERT /*+ append */ INTO {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_127CASE
     (                                                                             
      PROD_ID, PLAN_ID, OPE_NO, EQP_ID, PASS_FLG_PRC,                              
      PASS_FLG_MFG, PRC_UPT_USER, SGS_FLAG, OPE_SEQ                                
     )                                                                             
     SELECT I.PROD_ID,                                                           
              I.PLAN_ID,                                                           
              I.OPE_NO,                                                            
              I.EQP_ID,                                                            
