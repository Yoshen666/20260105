def getQtimeGradeData(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                      used_table_dict=None):
    sql = """
        INSERT INTO {tempdb}APS_TMP_ETL_RLS_QTIME_GRADE(                              
            PROD_ID, PLAN_ID, FROM_OPE_NO, TO_OPE_NO                                                           
        )                                                                          
         WITH GARDE_DATA AS (                                                                                                                                                                                                              
          SELECT C.PRODSPEC_ID,C.FLOW_KEY,C.FROM_OPE_NO,C.TO_OPE_NO,
                   COALESCE(C.CUSTOM_FIELD,'A') AS CUSTOM_FIELD,
                   ROW_NUMBER() OVER(PARTITION BY C.PRODSPEC_ID,C.FLOW_KEY,C.FROM_OPE_NO,C.TO_OPE_NO ORDER BY C.CUSTOM_FIELD) AS RNT                                                
          FROM APS_SYNC_QTIME_CONSTRAINT.APS_SYNC_QTIME_CONSTRAINT C                                                                                                                                       
         )                                                                                                                                                       
         SELECT PRODSPEC_ID,FLOW_KEY,FROM_OPE_NO,TO_OPE_NO                                                                    
         FROM GARDE_DATA                                                                                                                                         
         WHERE RNT = 1                                                    
         AND CUSTOM_FIELD NOT IN ('A','B')        
       """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_QTIME_GRADE",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_QTIME_GRADE")

    sql = """
          INSERT INTO {tempdb}APS_TMP_ETL_RLS_QTIME_GRADE_FLOW(                                 
            PLAN_ID, PROD_ID, TRIGGER_OPE_NO, TARGET_OPE_NO, TRIGGER_STEP_ID, TARGET_STEP_ID                                                                   
         )                                                                                  
         SELECT DISTINCT G.PLAN_ID,                                                                                                                                                                  
                G.PROD_ID,                                                                                                                                                           
                G.FROM_OPE_NO,                                                                                                                                                           
                G.TO_OPE_NO,                                                                                                                                                            
                FIRST_VALUE(F.STEP_ID IGNORE NULLS)                                                                                                                                         
                OVER(PARTITION BY F.PROD_ID, F.PLAN_ID, G.FROM_OPE_NO, G.TO_OPE_NO ORDER BY F.STEP_ID ROWS BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING) TRIGGER_STEP_ID,     
                LAST_VALUE(F.STEP_ID IGNORE NULLS)                                                                                                                                          
                OVER(PARTITION BY F.PROD_ID, F.PLAN_ID, G.FROM_OPE_NO, G.TO_OPE_NO ORDER BY F.STEP_ID ROWS BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING) TARGET_STEP_ID       
         FROM {tempdb}APS_TMP_ETL_RLS_QTIME_GRADE G                                                                                                                                                 
         INNER JOIN APS_ETL_FLOW.APS_ETL_FLOW F                                                                                                                                                         
         ON F.PLAN_ID = G.PLAN_ID
         AND F.PROD_ID = G.PROD_ID                                                                                                                                                                                                                                                                                                                       
         AND (CASE WHEN F.OPE_NO = G.FROM_OPE_NO THEN 1                                                                                                                                  
         WHEN F.OPE_NO = G.TO_OPE_NO THEN 1                                                                                                                                             
           ELSE 0 END = 1)        
          """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_QTIME_GRADE_FLOW",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_QTIME_GRADE_FLOW")


