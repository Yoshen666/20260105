           ,R.STEP_ID                                                                                                                                             
           ,R.OPE_NO                                                                                                                                              
           ,R.TARGET_STEP_ID                                                                                                                                      
           ,R.TARGET_OPE_NO                                                                                                                                       
           ,R.PLAN_ID                                                                                                                                             
           ,R.TARGET_PLAN_ID                                                                                                                                      
           ,R.TARGET_TOOLG_ID                                                                                                                                     
           ,R.EQP_ID                                                                                                                                              
           ,R.PROD_ID                                                                                                                                             
           ,R.FLOW_RECIPE                                                                                                                                         
           ,R.PPID                                                                                                                                                
           ,R.PHOTO_FLAG                                                                                                                                          
           ,R.RETICLE_ID                                                                                                                                          
           ,R.CHAMBER_SET                                                                                                                                         
