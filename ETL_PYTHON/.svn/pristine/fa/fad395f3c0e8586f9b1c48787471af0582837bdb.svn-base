         FROM APS_TR_CSFRINHIBIT.APS_TR_CSFRINHIBIT I, {tempdb}APS_TMP_ETL_RLS_EQPRCPRTC_RESULT  W                            
         WHERE 1=1                                                                     
         AND W.CHIPBODY =  I.CHIPBODY                                                                                 
