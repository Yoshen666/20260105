    sql = """
            INSERT /*+ append */ INTO  {tempdb}APS_TMP_ETL_RLS_NPW_RECIPE_GROUP1                                                                                                                 
            (TOOL_ID, RECIPE_ID, GROUP_ID) 
            WITH RESULT_DATA AS (
                SELECT DISTINCT EQP_ID,PPID
                FROM {tempdb}APS_TMP_ETL_RLS_TR_GLOBAL_RESULT
            )                                                                                                                                                                          
            SELECT D.EQP_ID,D.PPID,
                   CASE WHEN MAX(G.TOOL_ID) IS NOT NULL THEN MAX(G.GROUP_ID) ELSE MAX(G1.GROUP_ID) END AS GROUP_ID 
            FROM RESULT_DATA D                                    
            LEFT JOIN {tempdb}APS_TMP_ETL_RLS_NPW_RECIPE_GROUP G
            ON G.TOOL_ID = D.EQP_ID AND D.PPID = G.RECIPE_ID
            INNER JOIN {tempdb}APS_TMP_ETL_RLS_NPW_RECIPE_GROUP G1
            ON G1.TOOL_ID = D.EQP_ID AND INSTR(D.PPID,G1.RECIPE_ID) = 1 
