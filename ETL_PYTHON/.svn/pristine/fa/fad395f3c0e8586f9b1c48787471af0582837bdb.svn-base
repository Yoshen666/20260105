                ,W.RETICLE_ID                                                                                                                                                            
                ,W.FLOW_RECIPE                                                                                                                                                           
                ,W.SET_STATUS6                                                                                                                                                           
                ,W.SET_STATUS7                                                                                                                                                           
                ,W.SET_STATUS5                                                                                                                                                           
                ,W.BATCH_SIZE_MAX                                                                                                                                                        
                ,W.BATCH_SIZE_MIN                                                                                                                                                        
                ,W.QTY                                                                                                                                                                     
                ,W.BATCH_NAME,W.CASSETE_ID,W.BATCH_ID,W.EQP_CHAMBER_FLAG,W.SUB_LOT_TYPE, W.MODULE                                                                                        
                ,W.SET_STATUS2                                                                                                                                                           
                ,W.SET_STATUS18 AS SET_STATUS18                                                   
                ,W.IS_BTATH_FLAG                                                                                                                                                         
                ,CASE WHEN AEPR.PLAN_ID IS NOT NULL AND AEPR.PLAN_ID <> ''                                                                                                                                      
                           AND W.LOT_PROC_STATE <> 'Processing' THEN AEPR.RTD_QTIME||';' END AS SET_STATUS19                                                                             
                ,W.SET_STATUS20, W.LOT_PROC_STATE,W.TRANS_STATE
                ,SUBSTRING(W.TARGET_PLAN_ID, 1, POSITION('.' IN (W.TARGET_PLAN_ID))-1) AS CHIP_PLAN_ID                                                                                                                                      
             FROM {tempdb}APS_TMP_ETL_RLS_RTD_QTIME   W                                                                                                                                            
            -- 针对黄光区加的新版RTDQtime的逻辑
             LEFT JOIN APS_ETL_PH_RTDQTIME.APS_ETL_PH_RTDQTIME AEPR ON AEPR.PLAN_ID = W.TARGET_PLAN_ID 
                                                                       AND AEPR.OPE_NO = W.TARGET_OPE_NO  
                                                                       AND AEPR.PROD_ID = W.PROD_ID                  
                                                                       AND W.LOT_ID = AEPR.LOT_ID                                  

    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_PH_RTD_QTIME",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_PH_RTD_QTIME")


def getChamberRuleInhibit(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                          used_table_dict=None):
    sql = """
       INSERT INTO {tempdb}APS_TMP_ETL_RLS_MFG_CHAMBER_RULE1(                                                                 
             M_RECIPE, TARGET_OPE_NO, PROD_ID, TARGET_PLAN_ID,EQP_ID,PPID,EQP_CHAMBER_FLAG                                                                                              
        )                                                                                                                   
        SELECT DISTINCT M_RECIPE, TARGET_OPE_NO,PROD_ID,TARGET_PLAN_ID,EQP_ID,PPID,EQP_CHAMBER_FLAG
            FROM {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME
        """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_MFG_CHAMBER_RULE1",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_MFG_CHAMBER_RULE1")

    #  防止出现多个*和实际有值的冲突，以严格的为准
    sql = """
           INSERT INTO {tempdb}APS_TMP_ETL_RLS_MFG_CHAMBER_RULE2(                                                                 
                 M_RECIPE, TARGET_OPE_NO, PROD_ID, TARGET_PLAN_ID,EQP_ID,ACT_FLG                                                                                              
            )
            WITH CHAMBNER_DATA AS (
                SELECT W.M_RECIPE, W.TARGET_OPE_NO, W.PROD_ID, W.TARGET_PLAN_ID,W.EQP_ID,MTN.ACT_FLG,
                   ROW_NUMBER() OVER (PARTITION BY W.M_RECIPE, W.TARGET_OPE_NO, W.PROD_ID, 
                           W.TARGET_PLAN_ID,W.EQP_ID ORDER BY MTN.PROD_ID DESC,MTN.PLAN_ID DESC) AS RNT
                FROM {tempdb}APS_TMP_ETL_RLS_MFG_CHAMBER_RULE1 W 
                INNER JOIN {tempdb}APS_TMP_ETL_RLS_MFG_CHAMBER_RULE MTN 
                      ON MTN.EQP_ID = W.EQP_ID                                                                                                         
                      AND MTN.RECIPE_ID = W.M_RECIPE                                                                                                       
                      AND MTN.OPE_NO = W.TARGET_OPE_NO                                                                                                     
                      AND CASE WHEN MTN.PROD_ID = '*' THEN TRUE ELSE  MTN.PROD_ID = W.PROD_ID END                                                                       
                      AND INSTR(W.TARGET_PLAN_ID,(case when MTN.PLAN_ID = '*' then W.TARGET_PLAN_ID else MTN.PLAN_ID end))=1
            )                                                                                                                   
            SELECT M_RECIPE, TARGET_OPE_NO, PROD_ID, TARGET_PLAN_ID,EQP_ID,ACT_FLG
            FROM CHAMBNER_DATA W
            WHERE RNT = 1 
            """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_MFG_CHAMBER_RULE2",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_MFG_CHAMBER_RULE2")

    sql = """
        WITH CHAMBER_RULE_RESULT_DATA AS(
            SELECT W.M_RECIPE, W.TARGET_OPE_NO, W.PROD_ID, W.TARGET_PLAN_ID,W.EQP_ID,W.PPID                                                                                                                                
            ,CASE WHEN W.EQP_CHAMBER_FLAG='Chamber' AND TRIM(TME.EQP_ID) IS NOT NULL THEN TM.CH_ID ELSE                                                                               
                  (CASE WHEN W.EQP_CHAMBER_FLAG='Chamber' AND TRIM(TME.EQP_ID) IS NOT NULL THEN ETN.CH_ID ELSE                                                                                  
                  (CASE WHEN BTN.EQP_ID IS NOT NULL AND BTN.EQP_ID <> '' THEN BTN.CH_SET ELSE (CASE WHEN BTN2.EQP_ID IS NOT NULL AND BTN2.EQP_ID <> '' THEN BTN2.CH_SET ELSE (CASE WHEN BTN3.EQP_ID IS NOT NULL AND BTN3.EQP_ID <> '' THEN BTN3.CH_SET ELSE  
                  (CASE WHEN TTTI.EQP_ID IS NOT NULL AND TTTI.EQP_ID <> '' THEN TTTI.CH_SET ELSE (CASE WHEN TTTN.EQP_ID IS NOT NULL AND TTTN.EQP_ID <> '' THEN TTTN.CH_SET ELSE TST.CH_SET END) END) END) END) END) END)  END CHAMBER_SET               
            ,CASE WHEN W.EQP_CHAMBER_FLAG='Chamber' AND TME.EQP_ID IS NOT NULL AND TME.EQP_ID <> '' AND TM.EQP_ID IS NULL THEN 'NoChSet;'                                                                  
            ELSE (CASE WHEN W.EQP_CHAMBER_FLAG='Chamber' AND TM.EQP_ID IS NOT NULL AND TM.EQP_ID <> '' AND MTN.EQP_ID IS NULL AND position('DYB' in W.TARGET_PLAN_ID)=0 THEN 'ChUserFlg;'                    
            ELSE (CASE WHEN W.EQP_CHAMBER_FLAG='Chamber' AND MTN.EQP_ID IS NOT NULL AND MTN.EQP_ID <> '' AND MTN.PROD_ID <> '*' AND MTN.ACT_FLG = 'N' THEN 'ChUserFlg;'                                 
            ELSE (CASE WHEN W.EQP_CHAMBER_FLAG='Chamber' AND MTN.EQP_ID IS NOT NULL AND MTN.EQP_ID <> '' AND MTN.PROD_ID = '*' AND MTN.ACT_FLG = 'N' THEN 'ChUserFlg;' END) END)END)END AS SET_STATUS8  
            ,COALESCE(BTN.OTHER_MIN_CTRL,0) AS OTHER_MIN_BATCH_SIZE                                                                                                   
         FROM {tempdb}APS_TMP_ETL_RLS_MFG_CHAMBER_RULE1  W                                                                                                                                                
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_CHAMBER_RULE TM ON  TM.RECIPE_ID = W.M_RECIPE                                                                                                         
                                                          AND TM.EQP_ID = W.EQP_ID                                                                                                             
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_ONLY_EQP_CHAMBER_RULE TME ON TME.EQP_ID = W.EQP_ID                                                                                                    
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_EQP_CHAMBER_RULE  ETN ON ETN.EQP_ID = W.EQP_ID                                                                                                         
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_MFG_CHAMBER_RULE2  MTN ON MTN.EQP_ID = W.EQP_ID                                                                                                         
                                                          AND MTN.M_RECIPE = W.M_RECIPE                                                                                                       
                                                          AND MTN.TARGET_OPE_NO = W.TARGET_OPE_NO 
                                                          AND MTN.PROD_ID = W.PROD_ID 
                                                          AND MTN.TARGET_PLAN_ID = W.TARGET_PLAN_ID                                                                                                                                               
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_BOAT_CONSTRAINTS  BTN ON  BTN.OPE_NO = W.TARGET_OPE_NO                                                                                                 
                                                          AND BTN.PROD_ID = W.PROD_ID                                                                                                          
                                                          AND BTN.EQP_ID = W.EQP_ID                                                                                                             
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_BOAT_CONSTRAINTS2  BTN2 ON  BTN2.EQP_ID = W.EQP_ID                                                                                                     
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_BOAT_CONSTRAINTS3  BTN3 ON  BTN3.EQP_ID = W.EQP_ID                                                                                                     
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_SETTING_TANK_RULE  TST ON  TST.EQP_ID = W.EQP_ID                                                                                                       
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_TANK_RULE  TTTI ON  TTTI.EQP_ID = W.EQP_ID                                                                                                             
                                                            AND TTTI.CHAR_TYPE = 'IN' AND INSTR(W.PPID,TTTI.RECIPE) > 0                                                                           
         LEFT JOIN {tempdb}APS_TMP_ETL_RLS_TANK_RULE  TTTN ON  TTTN.EQP_ID = W.EQP_ID                                                                                                             
                                                            AND TTTN.CHAR_TYPE = 'NOT IN' AND INSTR(W.PPID,TTTN.RECIPE) = 0
        )
        UPDATE {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME W
        SET CH_SET = D.CHAMBER_SET, SET_STATUS8 = D.SET_STATUS8,OTHER_MIN_BATCH_SIZE = D.OTHER_MIN_BATCH_SIZE
        FROM  CHAMBER_RULE_RESULT_DATA D
        WHERE W.M_RECIPE = D.M_RECIPE
        AND W.TARGET_OPE_NO=D.TARGET_OPE_NO
        AND W.PROD_ID=D.PROD_ID
        AND W.TARGET_PLAN_ID=D.TARGET_PLAN_ID
        AND W.EQP_ID=D.EQP_ID
        AND W.PPID = D.PPID                                                                                                             

    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_PH_RTD_QTIME_CH",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_PH_RTD_QTIME")


def getAnomalyEwsLot(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                     used_table_dict=None):
    sql = """
    WITH ANOMALY_EWS_LOT AS (
        SELECT W.LOT_ID,W.TARGET_OPE_NO,W.EQP_ID,W.TARGET_PLAN_ID
             ,CASE WHEN LD.LOT_ID IS NOT NULL and LD.LOT_ID <>'' AND W.EQP_ID <> LD.EQP_ID THEN 'AnomalyLot;' END AS SET_STATUS21                                                                                                                             
             ,CASE WHEN LD2.EQP_ID IS NOT NULL and LD2.EQP_ID <>'' AND W.LOT_ID <> LD2.LOT_ID THEN 'AnomalyProd;' END AS SET_STATUS22                                                                                                                             
             ,W.LOT_PROC_STATE,W.TRANS_STATE                                                                                                                              
          FROM {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME  W                                                                                                                                            
          LEFT JOIN  {tempdb}APS_TMP_ETL_RLS_ANOMALY_EWS_LOT_DATA   LD ON  LD.LOT_ID = W.LOT_ID                                                                                                                         
                                                        AND LD.OPE_NO = W.TARGET_OPE_NO                                                                                                              
          LEFT JOIN  {tempdb}APS_TMP_ETL_RLS_ANOMALY_EWS_LOT_DATA   LD2 ON LD2.EQP_ID = W.EQP_ID                                                                                                                        
                                                        AND LD2.PRODSPEC_ID = W.PROD_ID
    )
    UPDATE {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME W
    SET SET_STATUS21 = D.SET_STATUS21, SET_STATUS22 = D.SET_STATUS22
    FROM ANOMALY_EWS_LOT D
    WHERE (D.SET_STATUS21 IS NOT NULL OR D.SET_STATUS22 IS NOT NULL)
    AND W.LOT_ID = D.LOT_ID AND W.TARGET_OPE_NO = D.TARGET_OPE_NO 
    AND W.EQP_ID = D.EQP_ID AND W.TARGET_PLAN_ID = D.TARGET_PLAN_ID
     """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into getAnomalyEwsLot",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_PH_RTD_QTIME")


