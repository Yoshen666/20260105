    # 先求 PassFlagPrc 为 Y 的
    sql = """
     INSERT INTO {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_CHIPBODY                                  
     (                                                                                           
       PROD_ID,PLAN_ID, OPE_NO, EQP_ID, CHIPBODY, PASS_FLG_PRC                                   
