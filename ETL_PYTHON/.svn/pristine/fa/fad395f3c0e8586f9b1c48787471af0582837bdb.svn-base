def InsertRlsTempData2Temp(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                           used_table_dict=None, pd_result=None):
    duck_db_memory.execute(
        "attach '{temp_process_db_file}' as dddd (READ_ONLY)".format(temp_process_db_file=pd_result))
    sql = """
           INSERT INTO {tempdb}APS_TMP_ETL_RLS_CSFR_PATH_RECIPE(                                             
                PROD_ID,CHIP_PLAN_ID,TARGET_OPE_NO,EQP_ID                                                                        
            )                                                                                            
            WITH MONITOR_RECIPE_DATA AS (
                SELECT DISTINCT W.PROD_ID,W.CHIP_PLAN_ID,W.TARGET_OPE_NO,W.EQP_ID
                FROM {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME W
           )
           SELECT W.PROD_ID,W.CHIP_PLAN_ID,W.TARGET_OPE_NO,W.EQP_ID
           FROM MONITOR_RECIPE_DATA W
           INNER JOIN {tempdb}APS_TMP_ETL_RLS_CSFR_PATH  CP ON W.PROD_ID = CP.PROD_ID                                                                                                                          
                                             AND W.CHIP_PLAN_ID = CP.PLAN_ID
                                             AND W.TARGET_OPE_NO = CP.OPE_NO                                                                                                                 
                                             AND W.EQP_ID = CP.EQP_ID                         
         """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_CSFR_PATH_RECIPE",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_CSFR_PATH_RECIPE")

    sql = """
      INSERT INTO {tempdb}APS_TMP_ETL_RLS_CHAMBER_RULE_DATA(                                             
           PROD_ID,M_RECIPE,TARGET_OPE_NO,EQP_ID, 
           CH_ID,CHAMBER_USER_FLAG                                                                       
       )                                                                                            
       WITH CHAMBER_RULE_DATA AS (
            SELECT DISTINCT W.PROD_ID,W.M_RECIPE,W.TARGET_OPE_NO,W.EQP_ID
            FROM {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME W
      )  
      SELECT W.PROD_ID,W.M_RECIPE,W.TARGET_OPE_NO,W.EQP_ID,
             TM.CH_ID,CHAMBER_USER_FLAG
      FROM CHAMBER_RULE_DATA W
      INNER JOIN {tempdb}APS_TMP_ETL_RLS_CHAMBER_RULE  TM ON TM.OPE_NO = W.TARGET_OPE_NO                                                                                                      
                                                  AND TM.PROD_ID = W.PROD_ID                                                                                                                     
                                                  AND TM.RECIPE_ID = W.M_RECIPE                                                                                                                  
                                                  AND TM.EQP_ID = W.EQP_ID                          
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_CHAMBER_RULE_DATA",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_CHAMBER_RULE_DATA")

    sql = """
        with data as (
            select w.lot_id,w.target_ope_no,w.eqp_id,w.target_plan_id,w.target_step_id,
                   CASE WHEN CP.EQP_ID IS NULL AND CI.EQP_ID IS NULL AND INSTR(W.TARGET_PLAN_ID,'DYB') <> 1 THEN 'CSFRInhibit;'                                                             
                               END as SET_STATUS1
            from {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME  W                                                                                                                                                 
            LEFT JOIN {tempdb}APS_TMP_ETL_RLS_CSFR_PATH_RECIPE  CP ON W.PROD_ID = CP.PROD_ID                                                                                                                          
                                 AND W.CHIP_PLAN_ID = CP.CHIP_PLAN_ID
                                 AND W.TARGET_OPE_NO = CP.TARGET_OPE_NO                                                                                                                 
                                 AND W.EQP_ID = CP.EQP_ID                                                                                                                        
            LEFT JOIN {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_RESULT  CI ON W.PROD_ID = CI.PROD_ID                                                                                                                       
                                 AND W.CHIP_PLAN_ID = CI.PLAN_ID
                                 AND W.TARGET_OPE_NO = CI.OPE_NO                                                                                                                 
                                 AND W.EQP_ID = CI.EQP_ID     
        )
        update {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME w             
        set SET_STATUS1 = d.SET_STATUS1
        from data d
        where d.SET_STATUS1 is not NULL 
        and w.lot_id = d.lot_id 
        and w.target_step_id =d.target_step_id 
        and w.target_plan_id =d.target_plan_id
        and w.eqp_id =d.eqp_id
        and w.target_ope_no =d.target_ope_no                          
        """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Update SET_STATUS1",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_PH_RTD_QTIME")

    sql = """
            with data as (
               select w.lot_id,w.target_ope_no,w.eqp_id,w.target_plan_id,w.target_step_id,
                      CASE WHEN W.OPE_NO = W.TARGET_OPE_NO AND W.LOT_PROC_STATE='RUN' THEN '' 
                       ELSE (CASE WHEN (ATN.EQP_ID IS NOT NULL) OR (GATN.PROD_ID IS NOT NULL AND GATN.PROD_ID <> '') 
                          THEN 'APC inhibit;' END) END AS SET_STATUS12 
               FROM {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME  W                                                                                                                                                                                                                                                                                                                                                                                              
               LEFT JOIN {tempdb}APS_TMP_ETL_RLS_APC_INHIBIT_LOT ATN ON  ATN.EQP_ID = W.EQP_ID AND ATN.PHYSICAL_RECIPE = W.PHYSICAL_RECIPE_ID
               LEFT JOIN {tempdb}APS_TMP_ETL_RLS_APC_INHIBIT_LOT_GLOBAL GATN ON  GATN.PROD_ID = W.PROD_ID AND GATN.OPE_NO = W.TARGET_OPE_NO 
               AND  CASE WHEN GATN.EQP_ID = '*' THEN TRUE ELSE GATN.EQP_ID = W.EQP_ID END
            )
            update {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME w             
            set SET_STATUS12 = 'APC inhibit;'                                                                                                                                                                                                                                                                                                                                                                                             
            FROM data d
            where d.SET_STATUS12 is not null
            and w.lot_id = d.lot_id 
            and w.target_step_id =d.target_step_id 
            and w.target_plan_id =d.target_plan_id
            and w.eqp_id =d.eqp_id
            and w.target_ope_no =d.target_ope_no                         
            """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Update SET_STATUS12",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_PH_RTD_QTIME")

    # 防止出现重复数据
    sql = """
        INSERT INTO {tempdb}APS_TMP_ETL_RLS_MASK_GROUP_MAPPLING(                                             
           OPE_NO,PRODSPEC_ID                                                                       
        )    
        SELECT OPE_NO,PRODSPEC_ID
        FROM APS_SYNC_MASK_GROUP_MAPPLING.APS_SYNC_MASK_GROUP_MAPPLING 
        GROUP BY OPE_NO,PRODSPEC_ID                        
        """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into  APS_TMP_ETL_RLS_MASK_GROUP_MAPPLING",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_MASK_GROUP_MAPPLING")

    # 对reticle有多条数据进行整合
    sql = """
     INSERT /*+ append */ INTO {tempdb}APS_TMP_ETL_RLS_FRDRBL_ID_DYB1
     (
        LOT_ID,OPE_NO,MAINPD_ID,DRBL_ID
      ) 
      SELECT DISTINCT W.LOT_ID,W.OPE_NO,W.MAINPD_ID,
          (
            SELECT STRING_AGG(DRBL_ID,';') OVER (PARTITION BY LOT_ID,OPE_NO,MAINPD_ID ORDER BY DRBL_ID) AS DRBL_ID
            FROM APS_SYNC_FRDRBL_ID_DYB.APS_SYNC_FRDRBL_ID_DYB D
            WHERE W.LOT_ID = D.LOT_ID
            AND W.OPE_NO = D.OPE_NO
            AND W.MAINPD_ID = D.MAINPD_ID
          )
      FROM APS_SYNC_FRDRBL_ID_DYB.APS_SYNC_FRDRBL_ID_DYB W
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into  APS_TMP_ETL_RLS_FRDRBL_ID_DYB1",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_FRDRBL_ID_DYB1")

    # APS_TMP_ETL_RLS_REWORK_COMING1 重复整合
    sql = """
        INSERT /*+ append */ INTO {tempdb}APS_TMP_ETL_RLS_REWORK_COMING1
        (
           OPE_NO,LOT_SON
         ) 
         SELECT DISTINCT W.OPE_NO,W.LOT_SON
         FROM {tempdb}APS_TMP_ETL_RLS_REWORK_COMING W
       """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into  APS_TMP_ETL_RLS_REWORK_COMING1",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_REWORK_COMING1")

    # APS_TMP_ETL_RLS_REWORK_COMING_SELF1 重复整合
    sql = """
           INSERT /*+ append */ INTO {tempdb}APS_TMP_ETL_RLS_REWORK_COMING_SELF1
           (
              OPE_NO,LOT_SON
            ) 
            SELECT DISTINCT W.OPE_NO,W.LOT_SON
            FROM {tempdb}APS_TMP_ETL_RLS_REWORK_COMING_SELF W
          """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into  APS_TMP_ETL_RLS_REWORK_COMING_SELF1",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_REWORK_COMING_SELF1")

    sql = """
     INSERT /*+ append */ INTO {tempdb}APS_TMP_ETL_RLS
     (
        PARENT_ID, LOT_ID, STEP_ID, OPE_NO, TARGET_STEP_ID, TARGET_OPE_NO, PLAN_ID, TARGET_PLAN_ID, PROD_ID, PD_ID,
        TARGET_TOOLG_ID, EQP_ID, QTY, M_RECIPE, PHYSICAL_RECIPE_ID, PHOTO_FLAG, RETICLE_ID, ACTIVE_RTCL, CHAMBER_SET,
        SET_STATUS, BATCH_SIZE_MAX, BATCH_SIZE_MIN, FLOW_RECIPE,OTHER_MIN_BATCH_SIZE,NON_WET_BATCH,MODULE 
      ) 
      WITH RLS_DATA AS (
            SELECT W.PARENT_ID                                                                                                                                                                   
              ,W.LOT_ID                                                                                                                                                                  
              ,W.OPE_NO                                                                                                                                                                  
              ,W.STEP_ID                                                                                                                                                                 
              ,W.TARGET_STEP_ID                                                                                                                                                          
              ,W.TARGET_OPE_NO                                                                                                                                                           
              ,W.PLAN_ID                                                                                                                                                                 
              ,W.TARGET_PLAN_ID                                                                                                                                                          
              ,W.PROD_ID                                                                                                                                                                 
              ,W.PD_ID                                                                                                                                                                   
              ,W.TARGET_TOOLG_ID                                                                                                                                                         
              ,W.EQP_ID                                                                                                                                                                  
              ,W.M_RECIPE                                                                                                                                                                
              ,W.PHYSICAL_RECIPE_ID                                                                                                                                                      
              ,W.PPID
              ,CASE WHEN INSTR(W.TARGET_OPE_NO,'.PP') > 0 THEN 'Y' ELSE 'N' END AS PHOTOFLAG                                                                                                                                                                                                                                                                                                   
              ,CASE WHEN TRIM(W.RETICLE_ID) IS NOT NULL AND W.RETICLE_ID<>' ' THEN W.RETICLE_ID ELSE (CASE WHEN TRIM(DY.DRBL_ID) IS NOT NULL THEN DY.DRBL_ID ELSE W.RETICLE_ID END)END AS RETICLE_ID          
              ,W.FLOW_RECIPE                                                                                                                                                             
              ,W.CH_SET        
              ,W.SET_STATUS1                                                                                                                                                            
              ,ERI.SET_STATUS2                                                                                                                                                             
               -- 加入逻辑：黄光区域V_MASK_GROUP_MAPPLING如果没有设置对应的MASK_GROUP可以不需要RETCILE，可以正常跑（除了DYB）   
               ,CASE WHEN INSTR(W.TARGET_PLAN_ID,'DYB') <> 1 AND (TRIM(W.RETICLE_ID) IS NULL OR W.RETICLE_ID=' ') AND TRIM(GM.PRODSPEC_ID) IS NOT NULL AND GM.PRODSPEC_ID <> '' AND W.TARGET_TOOLG_ID IN ('PK_KrF','PH_ArF','PU_I-Line','PI_Immer')  THEN 'RMS_NoReticle;'   
               ELSE (CASE WHEN STARTS_WITH(W.TARGET_PLAN_ID,'DYB') AND TRIM(DY.DRBL_ID) IS NULL AND W.TARGET_TOOLG_ID IN ('PK_KrF','PH_ArF','PU_I-Line','PI_Immer') THEN 'RMS_NoReticle;' END)	   END AS SET_STATUS4                            
              ,W.SET_STATUS6                                                                                                                                                
              ,W.SET_STATUS7                                                                                                                                                  
              ,W.SET_STATUS8                                                             
               -- 如果封Batch站点的Batch_id 为null，则不需要限制
              ,CASE WHEN W.OPE_NO = W.TARGET_OPE_NO AND RF.LOT_ID IS NULL AND W.IS_BTATH_FLAG = 'N' THEN 'ResourceFlag;'                                                                   
                WHEN W.OPE_NO = W.TARGET_OPE_NO AND RF.LOT_ID IS NULL AND W.BATCH_ID IS NOT NULL AND W.BATCH_ID <> '' AND W.IS_BTATH_FLAG = 'Y' THEN  'ResourceFlag;'                                   
                   END AS SET_STATUS9                                                                                                                                                
               -- 特殊LOT指定机群
              ,CASE WHEN LTN.LOT_ID IS NOT NULL AND LTN.LOT_ID <> '' AND LTN.TOOLG_ID <> W.TARGET_TOOLG_ID THEN 'LotEqpg;' END AS SET_STATUS10                                                                
               -- 针对Rework Coming绑机的逻辑
              ,CASE WHEN  RCT.OPE_NO IS NOT NULL AND RCT.OPE_NO <> '' AND NOT EXISTS (SELECT 1 FROM {tempdb}APS_TMP_ETL_RLS_REWORK_COMING C 
                               WHERE C.OPE_NO = W.TARGET_OPE_NO AND C.LOT_SON = W.LOT_ID AND C.EQP_ID = W.EQP_ID)  THEN 'ReworkComingInhibit;'                                                                                 
              ELSE (CASE WHEN RCST.OPE_NO IS NOT NULL AND RCST.OPE_NO <> '' AND NOT EXISTS (SELECT 1 FROM {tempdb}APS_TMP_ETL_RLS_REWORK_COMING_SELF C1 
                               WHERE C1.OPE_NO = W.TARGET_OPE_NO AND C1.LOT_SON = W.LOT_ID AND C1.EQP_ID = W.EQP_ID)  THEN 'ReworkComingInhibit;' END)                                                                      
              END AS SET_STATUS11      
             --如果是RUN状态，则不需要考量 
              ,W.SET_STATUS12      
              ,CASE WHEN W.TRANS_STATE NOT IN ('EO','SO','SI','EI') THEN 'TransStateInhibit;' 
                    WHEN (TTN.LOT_ID IS NOT NULL AND TTN.LOT_ID <> '') AND (LENGTH(W.TRANS_STATE) < 2 OR SUBSTRING(W.TRANS_STATE,2,1) <>'O') THEN 'TransStateInhibit;' END AS SET_STATUS13  
              ,CASE WHEN (W.FLOW_RECIPE IS NULL AND W.FLOW_RECIPE <> '') THEN 'LCRecipeNotExists;' END AS SET_STATUS14                                                                                             
              ,CASE WHEN (BTTN.BATCH_NAME IS NOT NULL AND BTTN.BATCH_NAME <> '') THEN 'FlowBatchPathFlag;' END AS SET_STATUS15                                                                                       
              ,CASE WHEN (MTTN.LOT_ID IS NOT NULL AND MTTN.LOT_ID <> '') THEN 'MultiLotInhibit;' END AS SET_STATUS16                                                                                             
              ,CASE WHEN (SFTN.LOT_ID IS NOT NULL AND SFTN.LOT_ID <> '') THEN 'RTDFoupRecipe;' END AS SET_STATUS17                                                                                               
               -- 目前只卡控DF、WET
              ,W.SET_STATUS18 
              ,W.SET_STATUS19
              ,W.SET_STATUS20,W.SET_STATUS21,W.SET_STATUS22   
              ,W.SET_STATUS23                                                                                                                                                             
              ,W.SET_STATUS5                                                                                                                                                             
              ,W.BATCH_SIZE_MAX                                                                                                                                                          
              ,W.BATCH_SIZE_MIN                                                                                                                                                          
              ,W.QTY                                                                                                                                                                     
              ,COALESCE(BTN.OTHER_MIN_CTRL,0) AS OTHER_MIN_BATCH_SIZE                                                                                                                         
               -- 非Batch的WET机台(Y非Batch、N为Batch)
              --,CASE WHEN W.MODULE='WET' AND W.IS_BTATH_FLAG = 'N' THEN 'Y' ELSE 'N' END AS NON_WET_BATCH  
              ,CASE WHEN W.IS_BTATH_FLAG = 'N' THEN 'Y' ELSE 'N' END AS NON_WET_BATCH                                                                               
              ,W.MODULE    
        --modify by xiecheng for version up                                                                                                                                                              
        FROM {tempdb}APS_TMP_ETL_RLS_PH_RTD_QTIME  W                                                                                                                                                 
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_RESOURCE_FLAG RF ON RF.LOT_ID = W.LOT_ID AND RF.OPE_NO = W.TARGET_OPE_NO AND RF.EQP_ID = W.EQP_ID AND RF.PLAN_ID = W.TARGET_PLAN_ID  AND RF.PROD_ID = W.PROD_ID  
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_LOT_TO_EQPG LTN ON LTN.LOT_ID = W.LOT_ID AND LTN.OPE_NO = W.TARGET_OPE_NO AND LTN.PLAN_ID = W.TARGET_PLAN_ID  AND LTN.PROD_ID = W.PROD_ID
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_MASK_GROUP_MAPPLING GM ON GM.OPE_NO = W.TARGET_OPE_NO AND GM.PRODSPEC_ID = W.PROD_ID
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_REWORK_COMING1 RCT ON  RCT.OPE_NO = W.TARGET_OPE_NO AND RCT.LOT_SON = W.LOT_ID AND STARTS_WITH(W.TARGET_PLAN_ID,'YY') AND W.OPE_NO <> W.TARGET_OPE_NO
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_REWORK_COMING_SELF1 RCST ON  RCST.OPE_NO = W.TARGET_OPE_NO AND RCST.LOT_SON = W.LOT_ID AND STARTS_WITH(W.TARGET_PLAN_ID,'YY') AND W.OPE_NO <> W.TARGET_OPE_NO  
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_FRDRBL_ID_DYB1 DY ON DY.LOT_ID = W.LOT_ID AND DY.OPE_NO = W.TARGET_OPE_NO AND DY.MAINPD_ID = W.TARGET_PLAN_ID
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_CHAMBER_RULE_DATA  TM ON  TM.TARGET_OPE_NO = W.TARGET_OPE_NO                                                                                                      
                                                  AND TM.PROD_ID = W.PROD_ID                                                                                                                     
                                                  AND TM.M_RECIPE = W.M_RECIPE                                                                                                                  
                                                  AND TM.EQP_ID = W.EQP_ID                                                                                                                       
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_BOAT_CONSTRAINTS BTN ON  BTN.OPE_NO = W.TARGET_OPE_NO                                                                                                
                                                  AND BTN.PROD_ID = W.PROD_ID                                                                                                                    
                                                  AND BTN.EQP_ID = W.EQP_ID                                                                                                                       
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_BOAT_CONSTRAINTS2 BTN2 ON  BTN2.EQP_ID = W.EQP_ID                                                                                                    
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_BOAT_CONSTRAINTS3 BTN3 ON  BTN3.EQP_ID = W.EQP_ID                                                                                                    
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_TRANS_STATE TTN ON  TTN.LOT_ID = W.LOT_ID AND TTN.OPE_NO = W.TARGET_OPE_NO                                                                           
                                                            AND TTN.MAINPD_ID = W.TARGET_PLAN_ID  AND TTN.PRODSPEC_ID = W.PROD_ID                                                            
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_BATCH_INFO BTTN ON  BTTN.BATCH_NAME = W.BATCH_NAME AND BTTN.OPE_NO = W.TARGET_OPE_NO                                                                 
                                                            AND BTTN.MAINPD_ID = W.TARGET_PLAN_ID  AND BTTN.PRODSPEC_ID = W.PROD_ID                                                          
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_MULTI_LOT MTTN ON  MTTN.CAST_ID = W.CASSETE_ID AND MTTN.OPE_NO = W.TARGET_OPE_NO                                                                     
                                                            AND MTTN.LOT_ID = W.LOT_ID                                                                                                       
        LEFT JOIN {tempdb}APS_TMP_ETL_RLS_FOUP_RECIPE SFTN ON  SFTN.TARGET_PLAN_ID = W.TARGET_PLAN_ID AND SFTN.TARGET_OPE_NO = W.TARGET_OPE_NO                                             
                                                            AND SFTN.LOT_ID = W.LOT_ID  AND SFTN.PROD_ID = W.PROD_ID 
        -- LEFT JOIN {tempdb}APS_TMP_ETL_RLS_APC_PILOT_RCP1 ERI 
        LEFT JOIN dddd.APS_TMP_ETL_RLS_APC_PILOT_RCP1 ERI 
         ON ERI.PROD_ID = W.PROD_ID
         AND ERI.M_RECIPE = W.M_RECIPE
         AND ERI.EQP_ID = W.EQP_ID
         AND ERI.SUB_LOT_TYPE = W.SUB_LOT_TYPE  
      )
      SELECT    R.PARENT_ID                                                                                                                                                                               
               ,R.LOT_ID                                                                                                                                                                                 
               ,R.STEP_ID                                                                                                                                                                                
               ,R.OPE_NO                                                                                                                                                                                 
               ,R.TARGET_STEP_ID                                                                                                                                                                         
               ,R.TARGET_OPE_NO                                                                                                                                                                          
               ,R.PLAN_ID                                                                                                                                                                                
               ,R.TARGET_PLAN_ID                                                                                                                                                                         
               ,R.PROD_ID                                                                                                                                                                                
               ,R.PD_ID                                                                                                                                                                                  
               ,R.TARGET_TOOLG_ID                                                                                                                                                                        
               ,R.EQP_ID                                                                                                                                                                                 
               ,R.QTY                                                                                                                                                                                    
               ,R.M_RECIPE                                                                                                                                                                               
               ,R.PHYSICAL_RECIPE_ID                                                                                                                                                                                             
               ,R.PHOTOFLAG                                                                                                                                                                              
               ,R.RETICLE_ID                                                                                                                                                                             
               ,'' AS ACTIVE_RTCL                                                                                                                                                                        
               ,R.CH_SET                                                                                                                                                                                                                                                                                                                                  
               ,CASE WHEN SET_STATUS1 IS NULL AND SET_STATUS2 IS NULL AND SET_STATUS4 IS NULL  AND SET_STATUS6 IS NULL AND SET_STATUS7 IS NULL 
                AND SET_STATUS8 IS NULL AND SET_STATUS9 IS NULL AND SET_STATUS10 IS NULL AND SET_STATUS11 IS NULL 
                AND SET_STATUS12 IS NULL AND SET_STATUS13 IS NULL AND SET_STATUS14 IS NULL AND SET_STATUS15 IS NULL
                AND SET_STATUS16 IS NULL AND SET_STATUS17 IS NULL AND SET_STATUS18 IS NULL AND SET_STATUS19 IS NULL
                AND SET_STATUS20 IS NULL AND SET_STATUS21 IS NULL AND SET_STATUS22 IS NULL AND SET_STATUS23 IS NULL 
                AND SET_STATUS5 IS NULL THEN 'ACTIVE'  
                ELSE 
                     CONCAT_WS('',SET_STATUS1,SET_STATUS2,SET_STATUS4,SET_STATUS6,SET_STATUS7,  
                     SET_STATUS8,SET_STATUS9,SET_STATUS10,SET_STATUS11,  
                     SET_STATUS12,SET_STATUS13,SET_STATUS14,SET_STATUS15, 
                     SET_STATUS16,SET_STATUS17,SET_STATUS18,SET_STATUS19,
                     SET_STATUS20,SET_STATUS21,SET_STATUS22,SET_STATUS23,  
                     SET_STATUS5)[:-1]
                END AS SET_STATUS                                                                                                                                                                          
               ,R.BATCH_SIZE_MAX                                                                                                                                                                         
               ,R.BATCH_SIZE_MIN                                                                                                                                                                         
               ,R.FLOW_RECIPE                                                                                                                                                                            
               ,R.OTHER_MIN_BATCH_SIZE                                                                                                                                                                   
               ,R.NON_WET_BATCH                                                                                                                                                                   
               ,R.MODULE 
      FROM RLS_DATA R                                                                                                                                                                               
    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into InsertRlsTempData2Temp",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS")


def InsertRlsTempByEqpgTemp(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                            used_table_dict=None):
    # 对数据进行过滤避免重复
    sql = """
       INSERT INTO {tempdb}APS_TMP_ETL_RLS_LOT_TO_EQPG_RECIPE_V ( 
           LOT_ID, OPE_NO, PROD_ID, PLAN_ID
       )
       SELECT DISTINCT F.LOT_ID,
                       F.OPE_NO,
                       F.PROD_ID,
                       F.PLAN_ID
       FROM {tempdb}APS_TMP_ETL_RLS_LOT_TO_EQPG_RECIPE  F
       """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_LOT_TO_EQPG_RECIPE_V",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_LOT_TO_EQPG_RECIPE_V")

    sql = """
    INSERT INTO {tempdb}APS_TMP_ETL_RLS_EQPG (                        
          PARENT_ID, LOT_ID, STEP_ID, OPE_NO, TARGET_STEP_ID, TARGET_OPE_NO, PLAN_ID, TARGET_PLAN_ID, PROD_ID, PD_ID,
          TARGET_TOOLG_ID, EQP_ID, QTY, M_RECIPE, PHYSICAL_RECIPE_ID, PHOTO_FLAG, RETICLE_ID, ACTIVE_RTCL, CHAMBER_SET,
          SET_STATUS, BATCH_SIZE_MAX, BATCH_SIZE_MIN, FLOW_RECIPE,OTHER_MIN_BATCH_SIZE, NON_WET_BATCH, MODULE,CH_CNT, BATCH_EQP_FLAG
    )                                                                          
     SELECT                                                                                                                                                                                         
           R.PARENT_ID                                                                                                                                                                              
           ,R.LOT_ID                                                                                                                                                                                
           ,R.STEP_ID                                                                                                                                                                               
           ,R.OPE_NO                                                                                                                                                                                
           ,R.TARGET_STEP_ID                                                                                                                                                                        
           ,R.TARGET_OPE_NO                                                                                                                                                                         
           ,R.PLAN_ID                                                                                                                                                                               
           ,R.TARGET_PLAN_ID                                                                                                                                                                        
           ,R.PROD_ID                                                                                                                                                                               
           ,R.PD_ID                                                                                                                                                                                 
           ,R.TARGET_TOOLG_ID                                                                                                                                                                       
           ,R.EQP_ID                                                                                                                                                                                
           ,R.QTY                                                                                                                                                                                   
           ,R.M_RECIPE                                                                                                                                                                              
           ,R.PHYSICAL_RECIPE_ID                                                                                                                                                                                            
           ,R.PHOTO_FLAG                                                                                                                                                                             
           ,R.RETICLE_ID                                                                                                                                                                            
           ,R.ACTIVE_RTCL                                                                                                                                                                           
           ,R.CHAMBER_SET                                                                                                                                                                            
           ,CASE WHEN T.LOT_ID IS NULL THEN R.SET_STATUS                                                                                                                                            
                 --ELSE (CASE WHEN T.PASS_FLG_PRC='Y' AND T.FLOW_RECIPE IS NOT NULL THEN 'ACTIVE'                                                                            
                 --   ELSE (CASE WHEN T.PASS_FLG_PRC='Y' AND T.FLOW_RECIPE IS NULL THEN 'LCRecipeNotExists'                                                                  
                  --      ELSE (CASE WHEN T.PASS_FLG_PRC='N' AND T.FLOW_RECIPE IS NULL                                                                                      
                  --            THEN 'LCRecipeNotExists;EqpgPrcInhibit' ELSE 'EqpgPrcInhibit' END) END) END)END  AS SET_STATUS
                  --modify by xiecheng for version up   
                  ELSE (CASE WHEN EXISTS (SELECT 1 FROM {tempdb}APS_TMP_ETL_RLS_LOT_TO_EQPG_RECIPE RR                                                                                              
                                WHERE RR.LOT_ID = R.LOT_ID AND RR.OPE_NO = R.TARGET_OPE_NO AND ACTIVE_FLAG ='Y' AND RR.EQP_ID = R.EQP_ID                                                           
                                      AND (RR.FLOW_RECIPE IS NOT NULL AND RR.FLOW_RECIPE <> '')) THEN 'ACTIVE' ELSE                                                                                                   
                                     (CASE WHEN R.FLOW_RECIPE IS NULL THEN 'LCRecipeNotExists' ELSE 'EqpgPrcInhibit' END) END) END AS SET_STATUS               
           ,R.BATCH_SIZE_MAX                                                                                                                                                                        
           ,R.BATCH_SIZE_MIN                                                                                                                                                                        
           ,R.FLOW_RECIPE                                                                                                                                                                           
           ,R.OTHER_MIN_BATCH_SIZE                                                                                                                                                                  
           ,R.NON_WET_BATCH, R.MODULE
           --modify by xiecheng for version up    
           ,CASE WHEN R.CHAMBER_SET IS NULL OR position('F' in R.EQP_ID) = 1 THEN 1 ELSE LENGTH(R.CHAMBER_SET) - LENGTH(REPLACE(REPLACE(R.CHAMBER_SET,';',''),',',''))+1 END AS CH_CNT                                                                                                                                                   
           ,CASE WHEN TOL.EQP_ID IS NOT NULL THEN 'Y' ELSE 'N' END AS BATCH_EQP_FLAG
