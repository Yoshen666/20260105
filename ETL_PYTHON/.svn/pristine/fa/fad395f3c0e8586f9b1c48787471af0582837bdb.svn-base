                     duck_db_memory=duck_db_memory,
                     ETL_Proc_Name=ETL_Proc_Name,
                     methodName="Insert Into APS_TMP_ETL_RLS_LOTSPECIAL_RESULT1",
                     sql=sql,
                     current_time=current_time,
                     update_table="APS_TMP_ETL_RLS_LOTSPECIAL_RESULT1")

    sql = """
    INSERT INTO  {tempdb}APS_TMP_ETL_RLS_LOT_SPECIAL (                                                                              
    PARENT_ID, LOT_ID,OPE_NO,STEP_ID,TARGET_STEP_ID,TARGET_OPE_NO,PLAN_ID,TARGET_PLAN_ID,
    PROD_ID,PD_ID,TARGET_TOOLG_ID,EQP_ID,M_RECIPE,PHYSICAL_RECIPE_ID,PPID,
    RETICLE_ID,FLOW_RECIPE,SET_STATUS6,SET_STATUS5,BATCH_SIZE_MAX,BATCH_SIZE_MIN,QTY,
    BATCH_NAME,CASSETE_ID,BATCH_ID,EQP_CHAMBER_FLAG,SUB_LOT_TYPE,MODULE,LOT_PROC_STATE,TRANS_STATE,NO_CH_SET
    )                                                                                                                   
     SELECT W.PARENT_ID                                                                                                                                                                  
          ,W.LOT_ID                                                                                                                                                                 
          ,W.OPE_NO                                                                                                                                                                 
          ,W.STEP_ID                                                                                                                                                                
          ,W.TARGET_STEP_ID                                                                                                                                                         
          ,W.TARGET_OPE_NO                                                                                                                                                          
          ,W.PLAN_ID                                                                                                                                                                
          ,W.TARGET_PLAN_ID                                                                                                                                                         
          ,W.PROD_ID                                                                                                                                                                
          ,W.PD_ID                                                                                                                                                                  
          ,W.TARGET_TOOLG_ID                                                                                                                                                        
          ,W.EQP_ID                                                                                                                                                                 
          ,W.M_RECIPE                                                                                                                                                               
          ,W.PHYSICAL_RECIPE_ID                                                                                                                                                     
          ,W.PPID                                                                                                                                                                   
          ,W.RETICLE_ID                                                                                         
          ,W.FLOW_RECIPE                                                                                                                                                            
          ,CASE WHEN LS.LOT_ID IS NOT NULL AND LS.LOT_ID <> '' AND NOT EXISTS (SELECT 1 FROM {tempdb}APS_TMP_ETL_RLS_LOTSPECIAL_RESULT R 
                  WHERE R.LOT_ID = W.LOT_ID  AND R.OPE_NO = W.TARGET_OPE_NO AND R.EQP_ID = W.EQP_ID)  THEN 'LotSpecial;' END AS SET_STATUS6                                                              
          ,CASE WHEN W.LOT_EQP_STATUS IS NOT NULL THEN W.LOT_EQP_STATUS||';' ELSE W.LOT_EQP_STATUS END AS SET_STATUS5                                                                                                                                          
          ,W.BATCH_SIZE_MAX                                                                                                                                                         
          ,W.BATCH_SIZE_MIN                                                                                                                                                         
          ,W.QTY                                                                                                                                                                    
          ,W.BATCH_NAME,W.CASSETE_ID,W.BATCH_ID,W.EQP_CHAMBER_FLAG,SUB_LOT_TYPE,W.MODULE,W.LOT_PROC_STATE,W.TRANS_STATE
          ,CASE WHEN W.EQP_CHAMBER_FLAG='Chamber' AND TME.EQP_ID IS NOT NULL AND TM.EQP_ID IS NULL AND W.LOT_PROC_STATE <> 'Processing' AND W.OPE_NO = W.TARGET_OPE_NO THEN 'NoChSet;'
                WHEN W.EQP_CHAMBER_FLAG='Chamber' AND TME.EQP_ID IS NOT NULL AND TM.EQP_ID IS NULL AND W.OPE_NO <> W.TARGET_OPE_NO THEN 'NoChSet;' ELSE 'ChSet' END AS NO_CH_SET                                                                                                      
     FROM  {tempdb}APS_TMP_ETL_RLS_EQPRCPRTC_RESULT  W                                                                                                                                                
     LEFT JOIN {tempdb}APS_TMP_ETL_RLS_LOTSPECIAL_RESULT1 LS ON  LS.LOT_ID = W.LOT_ID  AND LS.OPE_NO = W.TARGET_OPE_NO
     LEFT JOIN {tempdb}APS_TMP_ETL_RLS_CHAMBER_RULE TM ON  TM.RECIPE_ID = W.M_RECIPE AND TM.EQP_ID = W.EQP_ID
     LEFT JOIN {tempdb}APS_TMP_ETL_RLS_ONLY_EQP_CHAMBER_RULE TME ON TME.EQP_ID = W.EQP_ID                                                                                     
    """.format(tempdb=my_duck.get_temp_table_mark())
