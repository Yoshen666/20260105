     FROM {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_3CASE_INHIB RI,{tempdb}APS_TMP_ETL_RLS_CSFRIHBT_3CASE_NOTREW MI                                     
     ,{tempdb}APS_TMP_ETL_RLS_PATH_SETTING S                                                                                           
     WHERE MI.PROD_ID = RI.PROD_ID                                                                                          
     AND MI.OPE_NO = RI.REWORK_OPE_NO AND MI.EQP_ID = RI.EQP_ID                                                             
     AND S.TOOL_ID = RI.EQP_ID AND S.USE_FLAG_TYPE = 'Userflag(3)'                    

    """.format(tempdb=my_duck.get_temp_table_mark())
    my_duck.exec_sql(oracle_conn=oracle_conn,
