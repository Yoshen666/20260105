          , A.PASS_FLG_PRC                                                              
          , A.PASS_FLG_MFG                                                              
          , A.PRC_UPT_USER                                                              
      FROM (                                                                            
             SELECT * FROM {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_127CASE
             UNION                                                                     
             SELECT * FROM {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_5CASE
             UNION                                                                     
             SELECT * FROM {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_4CASE
             UNION                                                                     
             SELECT * FROM {tempdb}APS_TMP_ETL_RLS_CSFRIHBT_3CASE
