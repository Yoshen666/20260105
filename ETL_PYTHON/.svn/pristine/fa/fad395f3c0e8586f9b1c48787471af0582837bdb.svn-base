def InsertWipDataToToolDownTempTable(duck_db_memory=None, uuid="", current_time="", oracle_conn=None, ETL_Proc_Name="",
                                     used_table_dict=None):
    sql = """
       INSERT INTO {tempdb}APS_TMP_ETL_RLS_MEASURE_COMING_TOOL(                                     
