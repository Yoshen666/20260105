     SELECT                                                                        
        I.PROD_ID,                                                                 
        I.PLAN_ID,                                                                 
        I.OPE_NO,                                                                  
        I.EQP_ID,                                                                  
        I.PASS_FLG_PRC,                                                            
        I.PASS_FLG_MFG,                                                            
        I.PRC_UPT_USER,'', ''                                                      
     FROM {tempdb}APS_TMP_ETL_RLS_CSFR_IHBT I                                           
     INNER JOIN {tempdb}APS_TMP_ETL_RLS_PATH_SETTING S                                        
     ON S.TOOL_ID = I.EQP_ID                                                       
     AND S.USE_FLAG_TYPE = 'Userflag(7)'     
     WHERE I.PASS_FLG_PRC = 'Y'                                                    
     AND I.PASS_FLG_MFG = 'N'                                                      
     AND I.PRC_UPT_USER='EIWatchDog'                                              
    """.format(tempdb=my_duck.get_temp_table_mark())
